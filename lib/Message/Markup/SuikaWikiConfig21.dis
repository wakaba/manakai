IFClsDef:
  @IFQName: SWCFGElement
  @ClsQName: ManakaiSWCFGElement

  @ClsISA: ManakaiSWCFGNode

  @Description:
    @@lang:en
    @@@:
      An element node. 
      \
      {NOTE:: An element node have a value.  An element node can have 
              child elements if the value is a simple string (if any). 
              Future version of SuikaWikiConfig might allow a comment child. 
      \
      }

  @Attr:
    @@Name:expandedURI
    @@Type:SWCFGString
    @@Description:
      @@@lang:en
      @@@@:
        The expanded URI of the node. 
    @@NewIn21:
    @@Get:
      @@@UndeclaredPrefixException:
      @@@PerlDef:
        __CODE{expandNamespace}__;
        if (defined $self->{<H::mn:node>}->{<H::swcfg21:namespaceURI>}) {
          if ($self->{<H::mn:node>}
                   ->{<H::swcfg21:namespaceURI>} eq <Q::swcfg21:RealName>) {
            $r = $self->{<H::mn:node>}->{<H::swcfg21:shadowNamespaceURI>} .
                 $self->{<H::mn:node>}->{<H::swcfg21:shadowLocalName>};
          } else {
            $r = $self->{<H::mn:node>}->{<H::swcfg21:namespaceURI>} .
                 $self->{<H::mn:node>}->{<H::swcfg21:localName>};
          }
        } else {
          $r = $self->{<H::mn:node>}->{<H::swcfg21:localName>};
        }

  @Attr:
    @@Name:prefix
    @@Description:
      @@@lang:en
      @@@@:
        The namespace prefix of this node. 
    @@NewIn21:
    @@Get:
      @@@Type:SWCFGString
      @@@Description:
        @@@@lang:en
        @@@@@:The namespace prefix. 
      @@@InCase:
        @@@@Value:
          @@@@@is-null:1
        @@@@Type:SWCFGString
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            Either this node is not an element node or its namespace 
            is the default and no prefix is required. 
      @@@UndeclaredPrefixException:
      @@@PerlDef:
        __CODE{expandNamespace}__;
        my $node = $self->{<H::mn:node>};
        my $od = $self-><AG::SWCFGNode.ownerDocument>;
        my $defURI = $od-><AG::SWCFGDocument.defaultElementTypeNamespaceURI>;
        if (defined $node->{<H::swcfg21:namespaceURI>}) {
          if (not defined $defURI or
              $node->{<H::swcfg21:namespaceURI>} ne $defURI or
              $node->{<H::swcfg21:localName>} eq 'Namespace' or
              $node->{<H::swcfg21:localName>} eq 'ElementTypeBinding' or
              $node->{<H::swcfg21:localName>} =~ /:/) {
            $r = $self-><M::SWCFGNode.lookupNamespacePrefix>
                                             ($node->{<H::swcfg21:namespaceURI>},
                                              make_new_binding => 1);
          } else {
            $r = null;
          }
        } else {  ## Null namespace
          if (defined $defURI or $node->{<H::swcfg21:localName>} =~ /:/ or
              $node->{<H::swcfg21:localName>} eq 'Namespace' or
              $node->{<H::swcfg21:localName>} eq 'ElementTypeBinding') {
            $r = $self-><M::SWCFGNode.lookupNamespacePrefix>
                                             ($node->{<H::swcfg21:namespaceURI>},
                                              make_new_binding => 1);
          } else {
            $r = null;
          }
        }

  @Attr:
    @@Name: qualifiedName
    @@Type: SWCFGString
    @@Description:
      @@@lang:en
      @@@@:
        The qualified name of the element type, i.e. the local name 
        with or with out the namespace prefix followed by a <CHAR::COLON>. 
    @@NewIn21:
    @@Get:
      @@@UndeclaredPrefixException:
      @@@PerlDef:
        __CODE{expandNamespace}__;
        my $node = $self->{<H::mn:node>};
        my $od = $self-><AG::SWCFGNode.ownerDocument>;
        my $defURI = $od-><AG::SWCFGDocument.defaultElementTypeNamespaceURI>;
        if (defined $node->{<H::swcfg21:namespaceURI>}) {
          my $nsURI = ($node->{<H::swcfg21:namespaceURI>} eq <Q::swcfg21:RealName>
                        ? $node->{<H::swcfg21:shadowNamespaceURI>}
                        : $node->{<H::swcfg21:namespaceURI>});
          my $ln = ($node->{<H::swcfg21:namespaceURI>} eq <Q::swcfg21:RealName>
                        ? $node->{<H::swcfg21:shadowlocalName>}
                        : $node->{<H::swcfg21:localName>});
          if (not defined $defURI or
              $nsURI ne $defURI or
              $ln eq 'Namespace' or
              $ln eq 'ElementTypeBinding' or
              $ln =~ /:/) {
            $r = $self-><M::SWCFGNode.lookupNamespacePrefix>
                                             ($node->{<H::swcfg21:namespaceURI>},
                                              make_new_binding => 1) .
                 ':' . $ln;
          } else {
            $r = $ln;
