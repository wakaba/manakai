  @ResourceDef:
    @@QName: expandETBinding
    @@rdf:type:
      dis2pm:BlockCode
    @@Description:
      @@@lang:en
      @@@@:
        Expands element type bindings. 
    @@ForCheck: ManakaiDOM|ForClass
    @@UndeclaredPrefixException:
    @@PerlDef:
      unless ($node->{<H::swcfg21:etExpanded>}) {
        my $__qn;
        if ($node->{<H::swcfg21:namespaceExpanded>}) {
          $__qn = $self-><M::ManakaiSWCFGNode.getNodeReference> ($node)
                  -><AG::SWCFGElement.realQualifiedName>;
        } else {
          $__qn = $node->{<H::swcfg21:localName>};

        }
        my $__od = $node->{<H::swcfg21:ownerDocument>};
        if ($__od->{<H::swcfg21:etBinding>}->{$__qn}) {
          my $b = $__od->{<H::swcfg21:etBinding>}->{$__qn};
          $node->{<H::swcfg21:namespaceURI>} = <Q::swcfg21:RealName>;
          if ($node->{<H::swcfg21:namespaceExpanded>}) {
            $node->{<H::swcfg21:localName>} = $__qn;
          } else {
            $node->{<H::swcfg21:namespaceExpanded>} = true;
          }
          unless ($b->{namespaceExpanded}) {
            if ($b->{shadowLocalName} =~ s/^([^:]*)://) {
              my $__prefix = $1;
              __DEEP{
                $b->{shadowNamespaceURI}
                   = $self-><M::ManakaiSWCFGNode.getNodeReference> ($__od)
                           -><M::SWCFGNode.lookupNamespaceURI>
                                 ($__prefix, make_new_binding => false,
                                  raise_prefix_exception => true);
              }__;
            }
            $b->{namespaceExpanded} = true;
          }
          $node->{<H::swcfg21:shadowNamespaceURI>} = $b->{shadowNamespaceURI};
          $node->{<H::swcfg21:shadowLocalName>} = $b->{shadowLocalName};
          for my $__p (qw/shadowContent shadowSibling/) {
            next unless $b->{$__p};
            my $cl = $self-><M::ManakaiSWCFGNode.getNodeReference> ($b->{$__p})
                        ->clone;
            my $__n = $node->{{shadowContent => <H::swcfg21:shadowContent>,
                               shadowSibling => <H::swcfg21:shadowSibling>}->{$__p}}
               = $cl->{<H::mn:node>};
            $node-><M::NodeStem.importTree> ($__n);
            $__n->{{shadowContent => <H::swcfg21:shadowParent>,
                    shadowSibling => <H::swcfg21:shadowSiblingOf>}->{$__p}} = $node;
          }
        }
        $node->{<H::swcfg21:etExpanded>} = true;
      }

  @ResourceDef:
    @@QName: expandNamespace
    @@rdf:type:
      dis2pm:BlockCode
    @@ForCheck: ManakaiDOM|ForClass
    @@Description:
      @@@lang:en
      @@@@:
        Expands qualified name into namespace URI and local name. 
    @@UndeclaredPrefixException:
    @@PerlDef:
      my $__node = $self->{<H::mn:node>};
      __CODE{expandETBinding:: $node => $__node}__;
      unless ($__node->{<H::swcfg21:namespaceExpanded>}) {
        if ($__node->{<H::swcfg21:localName>} =~ s/^([^:]*)://) {
          my $__prefix = $1;
          __DEEP{
            $__node->{<H::swcfg21:namespaceURI>}
                 = $self-><M::SWCFGNode.lookupNamespaceURI>
                                 ($__prefix, make_new_binding => false, 
                                  raise_prefix_exception => true);
          }__;
        } else {
          $__node->{<H::swcfg21:namespaceURI>}
                 = $__node->{<H::swcfg21:ownerDocument>}
                          ->{<H::swcfg21:defaultETNamespaceURI>};
        }
        $__node->{<H::swcfg21:namespaceExpanded>} = true;
      }
