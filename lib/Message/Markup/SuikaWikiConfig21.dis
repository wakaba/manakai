          $r = <ClassM::ManakaiSWCFGNode.newObject> ($self);
          $r->{<H::swcfg21:nodeType>} = '#fragment';
          $r->{<H::swcfg21:namespaceExpanded>} = true; ## Always t r u e
          $r->{<H::swcfg21:etExpanded>} = true; ## Always t r u e
        @@@@ImplNote:
          @@@@@lang:en
          @@@@@@:
            Required property: <Q::ownerDocument>, <Q::swcfg21:nodeType>. 
            \
            Optional properties: <Q::swcfg21:parent>, <Q::swcfg21:value>,
                  <Q::swcfg21:childNodes>, <Q::flag>, <Q::option>, <Q::swcfg21:shadowOwner>, 
                  <Q::swcfg21:shadowParent>, <Q::swcfg21:shadowSiblingOf>. 

  @mn:origin0: swcfg21|shadowOwner
  @mn:origin0: swcfg21|shadowParent
  @mn:origin0: swcfg21|shadowSiblingOf
##SWCFGDocumentFragment

IFClsDef:
  @IFQName: SWCFGChildNodeList
  @ClsQName: ManakaiSWCFGChildNodeList

  @ClsISA: ManakaiNode|ManakaiNodeRef||ManakaiDOM|Perl
  @AppISA:
    @@ForCheck: ManakaiDOM|ForClass
    @@ContentType:
      lang:Perl
    @@@:
      Tie::Array

  @Description:
    @@lang:en
    @@@:
      A child node list of a node. 

  @Attr:
    @@Name:length
    @@Type:
      DOMMain:unsigned-long::ManakaiDOM:all
    @@Operator:
      @@@@:FETCHSIZE
      @@@ContentType:
        lang:Perl
    @@Description:
      @@@lang:en
      @@@@:
        The number of child nodes, including those belong to shadow trees. 
    @@Get:
      @@@UndeclaredPrefixException:
      @@@PerlDef:
        my $node = $self->{<H::mn:node>};
        $r = @{$node->{<H::swcfg21:childNodes>}};
        __CODE{expandETBinding:: $node => $node,
                                   $self => {$self->{nodeClass}}}__;
        if ($node->{<H::swcfg21:shadowContent>}) {
          $r += @{$node->{<H::swcfg21:shadowContent>}->{<H::swcfg21:childNodes>}};
        }
        for my $c (@{$node->{<H::swcfg21:childNodes>}}) {
          __CODE{expandETBinding:: $node => $c,
                                   $self => {$self->{nodeClass}}}__;
          $r += @{$c->{<H::swcfg21:shadowSibling>}->{<H::swcfg21:childNodes>}}
             if $c->{<H::swcfg21:shadowSibling>};
        }

  @Method:
    @@Name:item
    @@Description:
      @@@lang:en
      @@@@:
        Returns a child node. 
    @@Operator:
      @@@@:FETCH
      @@@ContentType:
        lang:Perl
    @@NewIn21:
    @@Param:
      @@@Name:index
      @@@Type:
        DOMMain:long::ManakaiDOM:all
      @@@Description:
        @@@@lang:en
        @@@@@:
          The ordinal index of the node to retrieve, counting from zero. 
          A negative index identifies a node from the end of the list. 
    @@Return:
      @@@Type:SWCFGNode
      @@@Description:
        @@@@lang:en
        @@@@@:
          The <P::index>th child node in the list. 
      @@@InCase:
        @@@@Value:
          @@@@@is-null:1
        @@@@Type:SWCFGNode
        @@@@Description:
          @@@@@lang:en
          @@@@@@: 
            There is no <P::index>th node in the list. 
      @@@UndeclaredPrefixException:
      @@@PerlDef:
        if ($index < 0) {
          $index = $self-><AG::SWCFGChildNodeList.length> + $index;
        }
        my $node = $self->{<H::mn:node>};
        $r = $node->{<H::swcfg21:childNodes>}->[$index];
        unless ($r) {
          $index -= @{$node->{<H::swcfg21:childNodes>}};
          __CODE{expandETBinding:: $node => $node,
                                   $self => {$self->{nodeClass}}}__;
          $r = $node->{<H::swcfg21:shadowContent>}->{<H::swcfg21:childNodes>}->[$index]
            if $node->{<H::swcfg21:shadowContent>};
          unless ($r) {
            if ($node->{<H::swcfg21:shadowContent>}) {
              $index -= @{$node->{<H::swcfg21:shadowContent>}->{<H::swcfg21:childNodes>}};
            }
            for my $c (@{$node->{<H::swcfg21:childNodes>}}) {
              __CODE{expandETBinding:: $node => $c,
                                   $self => {$self->{nodeClass}}}__;
              if ($c->{<H::swcfg21:shadowSibling>}) {
                $r = $c->{<H::swcfg21:shadowSibling>}->{<H::swcfg21:childNodes>}->[$index];
                last if $r;
                $index -= @{$c->{<H::swcfg21:shadowSibling>}->{<H::swcfg21:childNodes>}};
              }
            }
          }
        }
        $r = $self->{nodeClass}
                  -><M::ManakaiSWCFGNode.getNodeReference> ($r) if $r;

  @IntMethod:
    @@ForCheck: ManakaiDOM|ForClass
    @@Operator:
      @@@@:TIEARRAY
      @@@ContentType:
        lang:Perl
    @@ManakaiDOM:isStatic:1
    @@Description:
      @@@lang:en
      @@@@:
        Binds a node list to an array.  This method is intended to 
        be called by Perl <Perl::tie> function. 
    @@Param:
      @@@Name: nodeList
      @@@Type: SWCFGChildNodeList
      @@@Description:
        @@@@lang:en
        @@@@@:
          A node list that is tied with an array. 
    @@Return:
      @@@Type: SWCFGChildNodeList
      @@@Description:
        @@@@lang:en
        @@@@@:
          The node list to which the array has bound, 
          i.e. <P::nodeList>.
      @@@PerlDef:
        $r = $nodeList;

  @IntMethod:
    @@ForCheck: ManakaiDOM|ForClass
    @@Operator:
      @@@@:@{}
      @@@ContentType:
        lang:Perl
    @@Description:
      @@@lang:en
      @@@@:
        Returns an array representation of this node list. 
        \
        {NOTE:: It is intended that most operations through the 
                array is as like ones in DOM Level 0 JavaScript 
                implementations as possible. 
        \
        }
    @@Return:
      @@@Type:
        Perl:ARRAY::ManakaiDOM:all
      @@@PerlDef:
        tie my @list, ref $self, $self;
        $r = \@list;

  @IntMethod:
    @@ForCheck: ManakaiDOM|ForClass
    @@Operator:
      @@@@:DESTROY
      @@@ContentType:
        lang:Perl
    @@Description:
      @@@lang:en
      @@@@:
        Redefines destructor for the class
        <Class::ManakaiNode|ManakaiNodeRef||ManakaiDOM|Perl>, 
        since the <PerlModule::Tie::Array> also has its own empty destructor. 
    @@Return:
      @@@Def:
        @@@@ContentType:
          lang:dis
        @@@@DISPerl:cloneCode:
          ManakaiNode|ManakaiNodeRef||ManakaiDOM|Perl.destroy
