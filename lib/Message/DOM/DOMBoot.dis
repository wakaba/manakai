Module:
  @QName:
    MDOM:DOMBoot
  @FullName:
    @@lang: en
    @@@: DOM Bootstrap Interfaces
  @Namespace:
    http://suika.fam.cx/~wakaba/archive/2005/2/DOMBoot#
  
  @Description:
    @@lang:en
    @@@:
      Manakai modules provide their bootstrap mechanism.  Some non-DOM 
      modules provide DOM-compatible method to initialize their 
      constructs. 
      \
      The <Module::MDOM:DOMBoot> module provides so fundamental DOM 
      interfaces that non-DOM modules don't have to link the heavy 
      DOM implementation modules. 
      \
      {NOTE:: It is inapproproate and impossible to load both 
              the DOM modules and this module. 
      \
      }

  @Author:
    @@FullName: Wakaba
    @@Mail: w@suika.fam.cx
  @License:
     license:Perl+MPL
  @Date:
    @@@:
      $Date: 2005/05/11 14:07:41 $
    @@ContentType:
      dis:Date.RCS
  
  @Require:
    @@Module:
      @@@Name: DISIDL
      @@@QName:
        DISlib:DISIDL
      @@@WithFor:
        ManakaiDOM:all
    @@Module:
      @@@Name: DISPerl
      @@@QName:
        DISlib:DISPerl
      @@@WithFor:
        ManakaiDOM:all
    @@Module:
      @@@Name: DOMMetaImpl
      @@@WithFor:
        ManakaiDOM:ManakaiDOMLatest
    @@Module:
      @@@Name: DOMBoot
      @@@WithFor:
        ManakaiDOM:ManakaiDOM
    @@Module:
      @@@Name: DOMBoot
      @@@WithFor:
        ManakaiDOM:ManakaiDOM1
    @@Module:
      @@@Name: DOMBoot
      @@@WithFor:
        ManakaiDOM:ManakaiDOM3
    @@Module:
      @@@Name: DOMBoot
      @@@WithFor:
        ManakaiDOM:ManakaiDOMLatest
  @DefaultFor:
    ManakaiDOM:ManakaiDOMLatest

