  @Method:
     @@Name:  hasFeature
     @@Param:
        @@@Name:  feature
        @@@Type:
          DOMMain:DOMString
     @@Param:
        @@@Name:  version
        @@@Type:
          DOMMain:DOMString
     @@Return:
