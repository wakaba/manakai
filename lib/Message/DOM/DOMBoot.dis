        @@@PerlDef: \## Dummy
  @Method:
     @@Name:  createDocumentType
     @@Param:
        @@@Name:  qualifiedName
        @@@Type:  
          DOMMain:DOMString
     @@Param:
        @@@Name:  publicId
        @@@Type:  
          DOMMain:DOMString
     @@Param:
        @@@Name:  systemId
        @@@Type:  
          DOMMain:DOMString
     @@For:
       ManakaiDOM:DOM2
     @@Return:
        @@@Type:  
          DOMXML:DocumentType
        @@@PerlDef: \## Dummy
  @Method:
     @@Name:  createDocument
     @@Param:
        @@@Name:  namespaceURI
        @@@Type:
          DOMMain:DOMString
     @@Param:
        @@@Name:  qualifiedName
        @@@Type:  
          DOMMain:DOMString
     @@Param:
        @@@Name:  doctype
        @@@Type:  
          DOMMain:any
     @@Return:
        @@@Type: 
          DOMCore:Document
        @@@PerlDef: \## Dummy
     @@For:
       ManakaiDOM:DOM2
        
  @Method:
     @@Name:  getFeature
     @@Param:
        @@@Name:  feature
        @@@Type:
          DOMMain:DOMString
     @@Param:
        @@@Name:  version
        @@@Type:
          DOMMain:DOMString
     @@For:
       ManakaiDOM:DOM3
     @@Return:
        @@@Type:  
          DOMMain:any::ManakaiDOM:all
        @@@PerlDef: \## Dummy
