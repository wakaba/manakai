Namespace:
  @infoset:
     http://www.w3.org/2001/04/infoset#
  @lang:
     http://suika.fam.cx/~wakaba/archive/2004/8/18/lang#
  @license:
     http://suika.fam.cx/~wakaba/archive/2004/8/18/license#
  @ManakaiDOMImplementationException:
     http://suika.fam.cx/~wakaba/archive/2004/8/4/manakai-dom-exception#
  @TreeCore:\
Module:
  @Name:DOMXML
  @BindingName:
    @@@: dom
    @@prefix:  w3c.org
    @@Type:
      lang:IDL-DOM
  @FullName:
    @@lang:en
    @@@: DOM XML Module
  @Spec:
    @@DOM3:
      http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-BBACDC08
  @Namespace:     
    http://suika.fam.cx/~wakaba/archive/2004/8/18/dom-core#
  
  @Description:
    @@lang:en
    @@@:
      The DOM XML Module (known as Extended Interface in DOM Level 1). 
      The interfaces defined in this module form 
      part of the DOM Core specification but objects that expose 
      these interfaces will never be encounted in an HTML-only 
      DOM implementation.
  @ImplNote:
    @@lang:en
    @@@:
      The DOM Core Module (<Feature:Core> feature) and the feature 
      <Feature:XMLVersion> with version <FeatureVer:1.0> must be supported.
  
  @Author:
    @@FullName:Wakaba
    @@Mail:w@suika.fam.cx
  @License:
     license:Perl+MPL
  @Date.RCS:
     $Date: 2004/10/09 05:32:30 $

  @Require:
    @@Module:
      @@@Name: DOMCore
      @@@Namespace:
        http://suika.fam.cx/~wakaba/archive/2004/8/18/dom-core#
    @@Module:
      @@@Name: ManakaiDOMXML
      @@@Namespace:
        http://suika.fam.cx/~wakaba/archive/2004/mdom-xml#
  
  @ConditionDef:
    @@Name: DOM1
  @ConditionDef:
    @@Name: DOM2
    @@ISA:DOM1
  @ConditionDef:
    @@Name: DOM3
    @@ISA:DOM2
    
  @Feature:
    @@Name: XML
    @@Version: 1.0
    @@Condition[list]:
      DOM1
      DOM2
      DOM3
    @@Spec:
      @@@DOM3[list]:
        http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-E067D597 
    @@Description:
      @@@lang:en
      @@@@:
        The DOM XML Module, Level 1.
    @@Contrib:
      DOMCore:DOMImplementation::DOM1
    @@Contrib:
      DOMCore:DOMImplementation::DOM2
    @@Contrib:
      DOMCore:DOMImplementation::DOM3
    @@Contrib:
      DOMCore:Node::DOM1
    @@Contrib:
      DOMCore:Node::DOM2
    @@Contrib:
      DOMCore:Node::DOM3
  @Feature:
    @@Name: XML
    @@Version: 2.0
    @@Condition[list]:
      DOM2
      DOM3
    @@Spec:
      @@@DOM3[list]:
        http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-E067D597
    @@Description:
      @@@lang:en
      @@@@:
        The DOM XML Module, Level 2.
    @@Contrib:
      DOMCore:DOMImplementation::DOM2
    @@Contrib:
      DOMCore:DOMImplementation::DOM3
    @@Contrib:
      DOMCore:Node::DOM2
    @@Contrib:
      DOMCore:Node::DOM3
  @Feature:
    @@Name: XML
    @@Version: 3.0
    @@Condition[list]:
      DOM3
    @@Spec:
      @@@DOM3[list]:
        http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-E067D597
    @@Description:
      @@@lang:en
      @@@@:
        The DOM XML Module, Level 3.
    @@Contrib:
      DOMCore:DOMImplementation::DOM3
    @@Contrib:
      DOMCore:Node::DOM3
  
  @Feature:
    @@Name: XMLVersion
    @@Version: 1.0
    @@Condition[list]:
      DOM3
    @@Spec:
      @@@DOM3[list]:
        http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#Document3-version
        http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-E067D597
    @@Description:
      @@@lang:en
      @@@@: The Extensible Markup Language (XML) 1.0. 
    @@ImplNote:
      @@@lang:en
      @@@@:
        The <Feature:XML>/<FeatureVer:3.0> feature must be supported 
        <SRC:DOM 3>.
    @@Contrib:
      DOMCore:DOMImplementation::DOM3
  @Feature:
    @@Name: XMLVersion
    @@Version: 1.1
    @@Condition[list]:
      DOM3
    @@Spec:
      @@@DOM3[list]:
        http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#Document3-version
        http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-E067D597
    @@Description:
      @@@lang:en
      @@@@: The Extensible Markup Language (XML) 1.1.
    @@ImplNote:
      @@@lang:en
      @@@@:
        The <Feature:XML>/<FeatureVer:3.0> feature must be supported 
        <SRC:DOM 3>.
    @@Contrib:
      DOMCore:DOMImplementation::DOM3

IF:
  @Name:  CDATASection
  @ISA:  Text
  @Spec:
    @@DOM3[list]:
      http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-667469212
  @Description:
    @@lang:en
    @@@:
      The CDATA section, i.e. the marked section with its effective status 
      of <XML:CDATA>.
  @ImplNote:
    @@lang:en
    @@@: 
      <IF:CDATASection> is a <IF:Text> is a <IF:CharacterData> is a <IF:Node>.
  
  @ReAttr:
    @@Name: nodeName
    @@Description:
       @@@lang:en
       @@@@: The node name.
    @@Spec:
      @@@DOM3[list]:
        http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-F68D095
        http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-1841493061
    @@NSVersion[list]:
      DOMCore:Node.prefix
      DOMCore:Node.localName
    @@Get:
      @@@Type: DOMString
      @@@InCase:
        @@@@Value:\#cdata-section
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            Always this value for the <IF:CDATASection> node.
      @@@DefaultValue: \#cdata-section
      @@@Def: \
    @@Redefine: Node
  @ReAttr:
    @@Name: nodeType
    @@Description:
      @@@lang:en
      @@@@: The type of this node.
    @@Get:
      @@@Type: unsigned-short
      @@@InCase:
        @@@@Label: CDATA_SECTION_NODE
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            The node is a <IF:CDATASection>.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@: $r = __CLASS{Node}__->CDATA_SECTION_NODE;
    @@Redefine: Node

  @IntMethod:
    @@Name: newObject
    @@Description:
      @@@lang:en
      @@@@:
        Construct an object for a new instance of the <IF:CDATASection> node.
    @@Return:
      @@@Type:
        ManakaiDOM:ManakaiDOMNodeObject
      @@@Description:
        @@@@lang:en
        @@@@@:
          The new object for the <IF:CDATASection> node.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@:
               ## Condition sensitive
          $r = __CLASS{Node}__->__INT{newObject}__;
          $r->{<Q:DOMCore:nodeType>} = <Q:DOMXML:CDATASection>;
          $r->{<Q:infoset:content>} = '';
     @@Level[list]:
       1
       2
       3
     @@ImplNote:
       @@@lang:en
       @@@@:
         Other properties required to be a legal <IF:CDATASection> object:
         <Q:DOMCore:ownerDocument>.
         \
         Other available properties: <Q:infoset:parent>, 
         <Q:infoset:content>.
## IF:CDATASection

IF:
  @Name:  DocumentType
  @ISA:  Node
  @Spec:
    @@DOM3[list]:
      http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-412266927
  @Description:
    @@lang:en
    @@@:
      A <IF:Node> corresponding to a document type declaration in the XML 
      document entity or the document type declaration information 
      item of the XML Infoset.
    \
      DOM Levels 1 to 3 do not support editing <IF:DocumentType> nodes so 
      that all <IF:DocumentType> nodes are read-only.
    \
      A <IF:DocumentType> node can be a child of the <IF:Document> node. 
      DOM Levels 2 or higher allows a <IF:DocumentType> node without 
      any parent.  <IF:DocumentType> nodes are not able to have 
      their child nodes (<IF:Entity>s and <IF:Notation>s are defined not 
      to have any parent node).
  @Attr:
     @@Name:  name
     @@Description:
       @@@lang:en
       @@@@: The document type name.
     @@NSVersion[list]:
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The document type name.
        @@@Def:
          @@@@Type:
            lang:Perl
          @@@@@:
            my $node = $self->{<Q:TreeCore:node>};
            if (defined $node->{<Q:infoset:localName>}) {
              $r = (defined $node->{<Q:infoset:prefix>} ?
                   $node->{<Q:infoset:prefix>} . ':' : '') . 
                   $node->{<Q:infoset:localName>};
            } else {
              $r = $node->{<Q:DOMCore:name>};
            }
            
  @Attr:
     @@Name:  entities
     @@Description:
       @@@lang:en
       @@@@:
         A collection containing the general entities, both external 
         and internal, declared in the DTD.  Parameter entities and 
         duplicating declarations are not contained.
     @@Get:
        @@@Type:
          ManakaiDOMXML:ManakaiDOMEntities
        @@@Description:
          @@@@lang:en
          @@@@@:
            A <IF:NamedNodeMap> containing <IF:Entity>s.
  @Attr:
     @@Name:  notations
     @@Description:
       @@@lang:en
       @@@@:
         A collection containing the notations declared in the DTD.
     @@Get:
        @@@Type:
          ManakaiDOMXML:ManakaiDOMNotations
        @@@Description:
          @@@@lang:en
          @@@@@:
            A <IF:NamedNodeMap> containing <IF:Notation>s.
  @Attr:
     @@Name:  publicId
     @@Description:
        @@@lang:en
        @@@@: The public identifier of the external subset.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The public identifier.
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            infoset:publicIdentifier
     @@Level[list]:  2
     @@SpecLevel:2
  @Attr:
     @@Name:  systemId
     @@Description:
       @@@lang:en
       @@@@: The system identifier of the external subset.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The system identifier.
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            infoset:systemIdentifier
     @@Level[list]:  2
     @@SpecLevel:2
  @Attr:
     @@Name:  internalSubset
     @@Description:
       @@@lang:en
       @@@@:
         The internal subset as a string, without delimiting square brackets.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The internal subset as a string.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@: There is no internal subset.
        @@@ImplNote:
          @@@@lang:en
          @@@@@:
            The actual content returned depends on how much information
            is available to the DOM implementation.  This may vary 
            depending on various parameters, including the XML 
            processor used to build the document.
     @@Level[list]:  2
     @@SpecLevel:2
  
  @ReMethod:
     @@Name:  lookupPrefix
     @@Description:
       @@@lang:en
       @@@@:
         Lookup the namespace prefix associated to the given namespace 
         URI, starting from this node.
     @@Param:
        @@@Name:  namespaceURI
        @@@Type:
          ManakaiDOM:ManakaiDOMNamespaceURI
        @@@Description:
          @@@@lang:en
          @@@@@: The namespace URI to look for.
     @@Return:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@:
            An associated namespace prefix.
          \
            If more than one prefix are associated to the 
            namespace URI, which is returned is implementation 
            dependent.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@:
              No namespace prefix is found.  Always <DOM:null> 
              is returned for <IF:DocumentType> nodes.
        @@@Def:
          @@@@Type:
            lang:Perl
          @@@@@: $r = null;
     @@Level[list]:  3
     @@SpecLevel:3
     @@Redefine:
        DOMCore:Node
  @ReAttr:
    @@Name: nodeName
    @@Description:
      @@@lang:en
      @@@@: The node name.
    @@NSVersion[list]:
      DOMCore:Node.prefix
      DOMCore:Node.localName
    @@Get:
      @@@Type: DOMString
      @@@Description:
        @@@@lang:en
        @@@@@: The document type name.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@: __INT{{name}}__;
    @@Redefine: Node
  @ReAttr:
    @@Name: nodeType
    @@Description:
      @@@lang:en
      @@@@: The type of this node.
    @@Get:
      @@@Type: unsigned-short
      @@@InCase:
        @@@@Label: DOCUMENT_TYPE_NODE
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            This node is a <IF:DocumentType>.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@: $r = __CLASS{Node}__->DOCUMENT_TYPE_NODE;
    @@Redefine: Node
  @ReAttr:
     @@Name:  textContent
     @@Description:
       @@@lang:en
       @@@@:
         The text content of this node and its descendants.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The text content of this node.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@:
              The text content is defined to be <DOM:null>.
        @@@Def:
          @@@@Type:
            lang:Perl
          @@@@@: $r = null;
     @@Set:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: Setting this attribute has no effect.
        @@@Warning:
          @@@@Type:
            ManakaiDOM:ManakaiDOMImplementationWarning
          @@@@Name: ATTR_SET_NO_EFFECT
          @@@@Description:
            @@@@@lang:en
            @@@@@@: Setting this attribute has no effect.
        @@@Def: \
     @@Level[list]: 3
     @@SpecLevel:3
     @@Redefine: Node

  @IntMethod:
    @@Name: newObject
    @@Description:
      @@@lang:en
      @@@@:
        Construct an object for a new instance of the <IF:DocumentType> node.
    @@Return:
      @@@Type:
        ManakaiDOM:ManakaiDOMNodeObject
      @@@Description:
        @@@@lang:en
        @@@@@:
          The new object for the <IF:DocumentType> node.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@:
               ## Condition sensitive
          $r = __CLASS{Node}__->__INT{newObject}__;
          $r->{<Q:DOMCore:nodeType>} = <Q:infoset:DocumentTypeDeclaration>;
     @@Level[list]:
       1
       2
       3
     @@ImplNote:
       @@@lang:en
       @@@@:
         Properties required for a <IF:Node> to be a legal 
         <IF:DocumentType>: either <Q:infoset:localName> or 
         <Q:DOMCore:name>, <Q:DOMCore:implementation>, 
         <Q:ManakaiDOM:implID>.
         \
         Other available properties: <Q:infoset:parent>, 
         <Q:infoset:publicIdentifier>, <Q:infoset:systemIdentifier>, 
         <Q:infoset:prefix>, <Q:DOMCore:read-only>, <Q:DOMCore:ownerDocument>.
## IF:DocumentType

IF:
  @Name:  Notation
  @ISA:  Node
  @Spec:
    @@DOM3[list]:
      http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-5431D1B9
  @Description:
    @@lang:en
    @@@:
      A notation declaration in the DTD or 
      the notation information item in the XML Infoset.
    \
      DOM Levels 1 to 3 do not support editing <IF:Notation> nodes so that 
      all <IF:Notation> nodes are read-only.
    \
      A <IF:Notation> node does not have any parent.
  @Attr:
     @@Name:  publicId
     @@Description:
        @@@lang:en
        @@@@: The public identifier of this notation.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The public identifier.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@: The public identifier was not specified.
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            infoset:publicIdentifier
  @Attr:
     @@Name:  systemId
     @@Description:
       @@@lang:en
       @@@@: The system identifier of this notation.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The system identifier.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@: The system identifier was not specified.
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            infoset:systemIdentifier
  
  @ReAttr:
    @@Name: baseURI
    @@Description:
      @@@lang:en
      @@@@:
        The base URI of this node.
        \
        For the <IF:Notation> node, it is the <InfosetP:declaration base URI> 
        of the notation information item.
    @@Get:
      @@@Type:
        ManakaiDOM:ManakaiDOMURI
      @@@Description:
        @@@@lang:en
        @@@@@:
          The absolute base DOM URI.
      @@@InCase:
        @@@@Value:
          @@@@@is-null:1
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            The DOM implementation was not able to 
            obtain an absolute URI.
      @@@Def:
        @@@@Type:
          lang:dis
        @@@@GetProp:
          infoset:declarationBaseURI
    @@SpecLevel:3
    @@Level:3
    @@Redefine:
      DOMCore:Node
  @ReMethod:
     @@Name:  lookupPrefix
     @@Description:
       @@@lang:en
       @@@@:
         Lookup the namespace prefix associated to the given namespace 
         URI, starting from this node.
     @@Param:
        @@@Name:  namespaceURI
        @@@Type:
          ManakaiDOM:ManakaiDOMNamespaceURI
        @@@Description:
          @@@@lang:en
          @@@@@: The namespace URI to look for.
     @@Return:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@:
            An associated namespace prefix.
          \
            If more than one prefix are associated to the 
            namespace URI, which is returned is implementation 
            dependent.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@:
              No namespace prefix is found.  Always <DOM:null> 
              is returned for <IF:Notation> nodes.
        @@@Def:
          @@@@Type:
            lang:Perl
          @@@@@: $r = null;
     @@Level[list]:  3
     @@SpecLevel:3
     @@Redefine:
        DOMCore:Node
  @ReAttr:
    @@Name: nodeName
    @@Description:
      @@@lang:en
      @@@@: The node name.
    @@NSVersion[list]:
      DOMCore:Node.prefix
      DOMCore:Node.localName
    @@Get:
      @@@Type: DOMString
      @@@Description:
        @@@@lang:en
        @@@@@: The notation name.
      @@@Def:
        @@@@Type:
          lang:dis
        @@@@GetProp:
          DOMCore:name
    @@Redefine: Node
  @ReAttr:
    @@Name: nodeType
    @@Description:
      @@@lang:en
      @@@@: The type of this node.
    @@Get:
      @@@Type: unsigned-short
      @@@InCase:
        @@@@Label: NOTATION_NODE
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            This node is a <IF:Notation>.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@: $r = __CLASS{Node}__->NOTATION_NODE;
    @@Redefine: Node
  @ReAttr:
     @@Name:  textContent
     @@Description:
       @@@lang:en
       @@@@:
         The text content of this node and its descendants.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The text content of this node.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@:
              The text content is defined to be <DOM:null>.
        @@@Def:
          @@@@Type:
            lang:Perl
          @@@@@: $r = null;
     @@Set:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: Setting this attribute has no effect.
        @@@Warning:
          @@@@Type:
            ManakaiDOM:ManakaiDOMImplementationWarning
          @@@@Name: ATTR_SET_NO_EFFECT
          @@@@Description:
            @@@@@lang:en
            @@@@@@: Setting this attribute has no effect.
        @@@Def: \
     @@Level[list]: 3
     @@SpecLevel:3
     @@Redefine: Node
 
  @IntMethod:
    @@Name: newObject
    @@Description:
      @@@lang:en
      @@@@:
        Construct an object for a new instance of the <IF:Notation> node.
    @@Return:
      @@@Type:
        ManakaiDOM:ManakaiDOMNodeObject
      @@@Description:
        @@@@lang:en
        @@@@@:
          The new object for the <IF:Notation> node.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@:
               ## Condition sensitive
          $r = __CLASS{Node}__->__INT{newObject}__;
          $r->{<Q:DOMCore:nodeType>} = <Q:infoset:Notation>;
     @@Level[list]:
       1
       2
       3
     @@ImplNote:
       @@@lang:en
       @@@@:
         Properties required for a <IF:Node> to be a legal 
         <IF:Notation>: <Q:DOMCore:name>, <Q:DOMCore:ownerDocument>.
         \
         Other available properties: <Q:infoset:declarationBaseURI>, 
         <Q:infoset:publicIdentifier>, <Q:infoset:systemIdentifier>, 
         <Q:DOMCore:read-only>.
## IF:Notation

IF:
  @Name:  Entity
  @ISA:  Node
  @Spec:
    @@DOM3[list]:
      http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-527DCFF2
  @Description:
    @@lang:en
    @@@:
      A general entity, not entity declaration, either parsed or unparsed, 
      or either internal or external.
    \
      When the replacement text of the entity is available, 
      the child list of the <IF:Entity> node represents the structure 
      of that replacement value (assuming that the replacement text would be 
      parsed in context of <XML:content>).  Otherwise, the child list is empty.
      Note that if the entity contains an unbound namespace prefix, 
      the <A:namespaceURI> of the corresponding node in the <IF:Entity> 
      node subtree is <DOM:null>.
    \
      DOM Levels 1 to 3 do not support editing <IF:Entity> nodes, so that 
      all <IF:Entity> nodes and their descendants are read-only.
    \
      An <IF:Entity> node does not have any parent.
  @Attr:
     @@Name:  publicId
     @@Description:
       @@@lang:en
       @@@@: The public identifier associated with the entity.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The public identifier.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@: The public identifier is not specified.
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            infoset:publicIdentifier
  @Attr:
     @@Name:  systemId
     @@Description:
        @@@lang:en
        @@@@: The system identifier associated with the entity.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The system identifier.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@: The system identifier is not specified.
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            infoset:systemIdentifier
  @Attr:
     @@Name:  notationName
     @@Description:
       @@@lang:en
       @@@@: The name of the notation for the entity.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The notation name.
        @@@ImplNote:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@: This entity is a parsed entity.
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            infoset:notationName
  @Attr:
     @@Name:  inputEncoding
     @@Description:
       @@@lang:en
       @@@@: The encoding used for this entity at the time of parsing.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@:
            The encoding used for this entity.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@:
              Either the entity is from the internal subset or 
              the input encoding is not known.
          @@@@ImplNote:
            @@@@@lang:en
            @@@@@@:
              Is it mean that even the internal entity declared in the external 
              entity might have the input encoding!?
         @@@Def:
           @@@@Type:
             lang:dis
           @@@@GetProp:
             infoset:characterEncodingScheme
     @@Level[list]:  3
     @@SpecLevel:3
  @Attr:
     @@Name:  xmlEncoding
     @@Description:
       @@@lang:en
       @@@@:
         The encoding of this entity, as specified in the text declaration.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The encoding name.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@:
              This is not an external parsed entity.
        @@@ImplNote:
          @@@@lang:en
          @@@@@: 
            The encoding declaration is required if the text declaration 
            is used, but the text declaration itself is not required. 
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            DOMCore:xmlEncoding
     @@Level[list]:  3
     @@SpecLevel:3
  @Attr:
     @@Name:  xmlVersion
     @@Description:
       @@@lang:en
       @@@@:
         The XML version number of this entity, as specified in the 
         text declaration.
     @@ImplNote:
        @@@lang:en
        @@@@:
          XML 1.0 documents cannot include the external parsed entities 
          other than those versioned as XML 1.0.  XML 1.1 document 
          can use both XML 1.0 and XML 1.1 entities, but both interpreted 
          as XML 1.1 entities.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The version number.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@: 
              This is not an external parsed entity.
        @@@ImplNote:
          @@@@lang:en
          @@@@@:
            The XML Specifications allows the text declaration omitted. 
            Does the DOM implementation need to provide the default value? 
            What's the default value?
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            infoset:version
     @@Level[list]:  3
     @@SpecLevel:1
     @@ImplNote:
       @@@lang:en
       @@@@:
         Need consideration for the internal structure of the 
         XML and text declarations.  See <A:Document.xmlEncoding> and 
         <A:Document.xmlVersion> attributes defined in the 
         <Module:DOMCore> module.
  
  @ReAttr:
    @@Name: baseURI
    @@Description:
      @@@lang:en
      @@@@:
        The base URI of this node.
        \
        For the <IF:Entity> node, it is the <InfosetP:declaration base URI> 
        of the unparsed entity information item.
    @@Get:
      @@@Type:
        ManakaiDOM:ManakaiDOMURI
      @@@Description:
        @@@@lang:en
        @@@@@:
          The absolute base DOM URI.
      @@@InCase:
        @@@@Value:
          @@@@@is-null:1
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            The DOM implementation was not able to 
            obtain an absolute URI.
      @@@Def:
        @@@@Type:
          lang:dis
        @@@@GetProp:
          infoset:declarationBaseURI
    @@SpecLevel:3
    @@Level:3
    @@Redefine:
      DOMCore:Node
  @ReMethod:
     @@Name:  lookupPrefix
     @@Description:
       @@@lang:en
       @@@@:
         Lookup the namespace prefix associated to the given namespace 
         URI, starting from this node.
     @@Param:
        @@@Name:  namespaceURI
        @@@Type:
          ManakaiDOM:ManakaiDOMNamespaceURI
        @@@Description:
          @@@@lang:en
          @@@@@: The namespace URI to look for.
     @@Return:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@:
            An associated namespace prefix.
          \
            If more than one prefix are associated to the 
            namespace URI, which is returned is implementation 
            dependent.
        @@@InCase:
          @@@@Value:
            @@@@@is-null:1
          @@@@Description:
            @@@@@lang:en
            @@@@@@:
              No namespace prefix is found.  Always <DOM:null> 
              is returned for <IF:Entity> nodes.
        @@@Def:
          @@@@Type:
            lang:Perl
          @@@@@: $r = null;
     @@Level[list]:  3
     @@SpecLevel:3
     @@Redefine:
        DOMCore:Node
  @ReAttr:
    @@Name: nodeName
    @@Description:
      @@@lang:en
      @@@@: The node name.
    @@NSVersion[list]:
      DOMCore:Node.prefix
      DOMCore:Node.localName
    @@Get:
      @@@Type: DOMString
      @@@Description:
        @@@@lang:en
        @@@@@: The entity name.
      @@@Def:
        @@@@Type:
          lang:dis
        @@@@GetProp:
          DOMCore:name
    @@Redefine: Node
  @ReAttr:
    @@Name: nodeType
    @@Description:
      @@@lang:en
      @@@@: The type of this node.
    @@Get:
      @@@Type: unsigned-short
      @@@InCase:
        @@@@Label: ENTITY_NODE
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            This node is an <IF:Entity>.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@: $r = __CLASS{Node}__->ENTITY_NODE;
    @@Redefine: Node

  @IntMethod:
    @@Name: newObject
    @@Description:
      @@@lang:en
      @@@@:
        Construct an object for a new instance of the <IF:Entity> node.
    @@Return:
      @@@Type:
        ManakaiDOM:ManakaiDOMNodeObject
      @@@Description:
        @@@@lang:en
        @@@@@:
          The new object for the <IF:Entity> node.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@:
               ## Condition sensitive
          $r = __CLASS{Node}__->__INT{newObject}__;
          $r->{<Q:DOMCore:nodeType>} = <Q:DOMXML:Entity>;
     @@Level[list]:
       1
       2
       3
     @@ImplNote:
       @@@lang:en
       @@@@:
         Properties required for a <IF:Node> to be a legal 
         <IF:Entity>: <Q:DOMCore:name>, <Q:DOMCore:ownerDocument>.
         \
         Other available properties: <Q:infoset:declarationBaseURI>, 
         <Q:DOMCore:read-only>,
         <Q:ManakaiDOM:isExpanded>, <Q:ManakaiDOM:isExternallyDeclared>, 
         <Q:ManakaiDOM:isExternalEntity>.
         \
         Other available properties (for external entity): 
         <Q:DOMCore:documentURI>, <Q:infoset:publicIdentifier>,
         <Q:infoset:systemIdentifier>, <Q:infoset:notationName>,
         <Q:infoset:version>, <Q:DOMCore:xmlEncoding>,
         <Q:infoset:characterEncodingScheme>,
         <Q:infoset:baseURI>.
## IF:Entity

IF:
  @Name:  EntityReference
  @ISA:  Node
  @Spec:
    @@DOM3[list]:
      http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-11C98490
  @Description:
    @@lang:en
    @@@:
      An entity reference.
    \
      When an <IF:EntityReference> node represents a reference to an <IF:Entity> 
      node, the subtree of the <IF:EntityReference> node is a copy of 
      the <IF:Entity> node subtree, except that when an entity contains 
      an unbound namespace prefix, the descendants of the 
      <IF:EntityReference> node may be bound to different namespace URIs.
    \
      When an <IF:EntityReference> node represents a reference to an 
      known entity, the node has no child node.
    \
      In DOM Levels 1 to 3, an <IF:EntityReference> node and all 
      descendants are read-only.
    \
      Note that <IF:EntitiReference> nodes may cause normalization 
      problems when the normalization is performed after 
      entity references are expanded.
  @ImplNote:
    @@lang:en
    @@@:
      Note that the character references and references to predefined 
      entities are considered to be expanded by the HTML or XML 
      processor.  Moreover, the XML processor may completely expand 
      references to entities.  (Does this clause in the DOM specification 
      mean that the character entity references in HTML document 
      should be expanded before the DOM tree constructed?)
  @ImplNote:
    @@lang:en
    @@@:
      The DOM Level 1 Specifications allow the lazy 
      evaluation of the children of the <IF:EntityReference>.
      For example, calling the <A:Node.childNodes> method might trigger the 
      evaluation.
  
  @ReAttr:
    @@Name: baseURI
    @@Description:
      @@@lang:en
      @@@@:
        The base URI of this node.
        \
        For the <IF:EntityReference> node, it is the 
        <InfosetP:declaration base URI> of the unexpanded entity 
        reference information item.
    @@Get:
      @@@Type:
        ManakaiDOM:ManakaiDOMURI
      @@@Description:
        @@@@lang:en
        @@@@@:
          The absolute base DOM URI.
      @@@InCase:
        @@@@Value:
          @@@@@is-null:1
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            The DOM implementation was not able to 
            obtain an absolute URI.
      @@@Def:
        @@@@Type:
          lang:dis
        @@@@GetProp:
          infoset:declarationBaseURI
    @@SpecLevel:3
    @@Level:3
    @@Redefine:
      DOMCore:Node
  @ReAttr:
    @@Name: nodeName
    @@Description:
      @@@lang:en
      @@@@: The node name.
    @@NSVersion[list]:
      DOMCore:Node.prefix
      DOMCore:Node.localName
    @@Get:
      @@@Type: DOMString
      @@@Description:
        @@@@lang:en
        @@@@@: The name of the entity referred.
      @@@Def:
        @@@@Type:
          lang:dis
        @@@@GetProp:
          DOMCore:name
    @@Redefine: Node
  @ReAttr:
    @@Name: nodeType
    @@Description:
      @@@lang:en
      @@@@: The type of this node.
    @@Get:
      @@@Type: unsigned-short
      @@@InCase:
        @@@@Label: ENTITY_REFERENCE_NODE
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            This node is an <IF:EntityReference>.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@: $r = __SUPER{Node}__->ENTITY_REFERENCE_NODE;
    @@Redefine: Node

  @IntMethod:
    @@Name: newObject
    @@Description:
      @@@lang:en
      @@@@:
        Construct an object for a new instance of the <IF:EntityReference> node.
    @@Return:
      @@@Type:
        ManakaiDOM:ManakaiDOMNodeObject
      @@@Description:
        @@@@lang:en
        @@@@@:
          The new object for the <IF:EntityReference> node.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@:
               ## Condition sensitive
          $r = __CLASS{Node}__->__INT{newObject}__;
          $r->{<Q:DOMCore:nodeType>} = <Q:DOMXML:EntityReference>;
     @@Level[list]:
       1
       2
       3
     @@ImplNote:
       @@@lang:en
       @@@@:
         Properties required for a <IF:Node> to be a legal 
         <IF:EntityReference>: <Q:DOMCore:name>, <Q:DOMCore:ownerDocument>.
         \
         Other available properties: <Q:infoset:declarationBaseURI>, 
         <Q:DOMCore:read-only>, <Q:infoset:parent>, <Q:infoset:children>, 
         <Q:ManakaiDOM:isExpanded>, <Q:ManakaiDOM:isExternallyDeclared>, 
         <Q:ManakaiDOM:isExternalEntity>.
         \
         Other available properties (for external entity): 
         <Q:infoset:baseURI>, <Q:DOMCore:documentURI>, 
         <Q:infoset:publicIdentifier>, <Q:infoset:systemIdentifier>.
# IF:EntityReference

IF:
  @Name:  ProcessingInstruction
  @ISA:  Node
  @Spec:
    @@DOM3[list]:
      http://www.w3.org/TR/2004/REC-DOM-Level-3-Core-20040407/core.html#ID-1004215813
  @Description:
    @@lang:en
    @@@:
      A processing instruction.  Note that the XML declaration and 
      text declarations are not considered as processing instructions.
  
  @Attr:
     @@Name:  target
     @@Description:
       @@@lang:en
       @@@@:
         The target of this processing instruction.
     @@NSVersion[list]:
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The target name.
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            infoset:target
  @Attr:
     @@Name:  data
     @@Description:
       @@@lang:en
       @@@@:
         The content of this processing instruction.
       \
         The content does not contain the whitespaces 
         delimiting with the target name.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@: The content.
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            infoset:data
        @@@ImplNote:
          @@@@lang:en
          @@@@@:
            Can <DOM:null> be returned (to represent <XML:<lt>?name?<gt>>)?
     @@Set:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@:
            The content.  It may contain the <SGML:PIC> (<XML:?<gt>>) but such 
            data result the document in unserializable.
        @@@Exception:
          @@@@Type: DOMException
          @@@@Name: NO_MODIFICATION_ALLOWED_ERR
          @@@@Description:
            @@@@@lang:en
            @@@@@@: This node is read-only.
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@SetProp:
            @@@@@@:
              infoset:content
            @@@@@CheckReadOnly:1
  
  @ReAttr:
    @@Name: baseURI
    @@Description:
      @@@lang:en
      @@@@:
        The base URI of this node.
    @@Get:
      @@@Type:
        ManakaiDOM:ManakaiDOMURI
      @@@Description:
        @@@@lang:en
        @@@@@:
          The absolute base DOM URI.
      @@@InCase:
        @@@@Value:
          @@@@@is-null:1
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            The DOM implementation was not able to 
            obtain an absolute URI.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@:
          $r = $self->{<Q:TreeCore:node>}
                    ->__SUPER{ManakaiDOMNodeObjectNode::}__::__INT{contentBaseURI}__
                                                                      (false);
    @@SpecLevel:3
    @@Level:3
    @@Redefine:
      DOMCore:Node
  @ReAttr:
    @@Name: nodeName
    @@Description:
      @@@lang:en
      @@@@: The node name.
    @@NSVersion[list]:
      DOMCore:Node.prefix
      DOMCore:Node.localName
    @@Get:
      @@@Type: DOMString
      @@@Description:
        @@@@lang:en
        @@@@@: 
          The target name, i.e. same as <A:ProcessingInstruction.target>.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@: __INT{{target}}__;
    @@Redefine: Node
  @ReAttr:
    @@Name: nodeType
    @@Description:
      @@@lang:en
      @@@@: The type of this node.
    @@Get:
      @@@Type: unsigned-short
      @@@InCase:
        @@@@Label: PROCESSING_INSTRUCTION_NODE
        @@@@Description:
          @@@@@lang:en
          @@@@@@:
            This node is a <IF:ProcessingInstruction>.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@: $r = __SUPER{Node}__->PROCESSING_INSTRUCTION_NODE;
    @@Redefine: Node
  @ReAttr:
     @@Name:  nodeValue
     @@Description:
       @@@lang:en
       @@@@: The value of this node.
     @@Get:
        @@@Type:  DOMString
        @@@Exception:
          @@@@Type: DOMException
          @@@@Name: DOMSTRING_SIZE_ERR
          @@@@Description:
            @@@@@lang:en
            @@@@@@: 
              The target data, i.e. same as <A:ProcessingInstruction.data>.
        @@@Def:
          @@@@Type: 
            lang:Perl
          @@@@@: __INT{{data}}__;
     @@Set:
        @@@Type:  DOMString
        @@@Exception:
          @@@@Type: DOMException
          @@@@Name: NO_MODIFICATION_ALLOWED_ERR
          @@@@Description:
            @@@@@lang:en
            @@@@@@: 
              This node is read-only.
        @@@Description:
          @@@@lang:en
          @@@@@:
            New node value.
        @@@Def:
          @@@@Type:
            lang:Perl
          @@@@@:
                           ## Condition sensitive
            __DEEP{ $self->__SUPER{ProcessingInstruction}__::data ($given) }__;
    @@Redefine: Node
  @ReAttr:
     @@Name:  textContent
     @@Description:
       @@@lang:en
       @@@@: The text content of this node and its descendants. 
     @@@ImplNote:
        @@@@lang:en
        @@@@@:
          <A:ProcessingInstruction.data> == 
          <A:ProcessingInstruction.nodeValue> == 
          <A:ProcessingInstruction.textContent>.
     @@Get:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@:
            The text content of this node, i.e. the <A:Node.nodeValue>.
        @@@Exception:
          @@@@Type: DOMException
          @@@@Name: DOMSTRING_SIZE_ERR
          @@@@Description:
            @@@@@lang:en
            @@@@@@:
              The attribute would return more characters than 
              fit in a <TYPE:DOMString> variable on the implementation 
              platform.
        @@@Def:
          @@@@Type: 
            lang:Perl
          @@@@@: __INT{{data}}__;
     @@Set:
        @@@Type:  DOMString
        @@@Description:
          @@@@lang:en
          @@@@@:
            The new text content of the node.
        @@@Exception:
          @@@@Type: DOMException
          @@@@Name: NO_MODIFICATION_ALLOWED_ERR
          @@@@Description:
            @@@@@lang:en
            @@@@@@: This node is read-only.
        @@@Def:
          @@@@Type:
            lang:Perl
          @@@@@:
                           ## Condition sensitive
            __DEEP{ $self->__SUPER{ProcessingInstruction}__::data ($given) }__;
     @@Level[list]:
        3
     @@SpecLevel:3
     @@Redefine: Node

  @IntMethod:
    @@Name: newObject
    @@Description:
      @@@lang:en
      @@@@:
        Construct an object for a new instance of the 
        <IF:ProcessingInstruction> node.
    @@Return:
      @@@Type:
        ManakaiDOM:ManakaiDOMNodeObject
      @@@Description:
        @@@@lang:en
        @@@@@:
          The new object for the <IF:ProcessingInstruction> node.
      @@@Def:
        @@@@Type:
          lang:Perl
        @@@@@:
               ## Condition sensitive
          $r = __CLASS{Node}__->__INT{newObject}__;
          $r->{<Q:DOMCore:nodeType>} = <Q:infoset:ProcessingInstruction>;
          $r->{<Q:infoset:content>} = '';
     @@Level[list]:
       1
       2
       3
     @@ImplNote:
       @@@lang:en
       @@@@:
         Properties required for a <IF:Node> to be a legal 
         <IF:ProcessingInstruction>: <Q:infoset:target>, 
         <Q:DOMCore:ownerDocument>.
         \
         Other available properties: <Q:infoset:baseURI>, 
         <Q:DOMCore:read-only>, <Q:infoset:parent>, <Q:infoset:content>.
## IF:ProcessingInstruction
    
