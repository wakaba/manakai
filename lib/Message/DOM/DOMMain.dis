        }__;
      }
      unless (<Code::DOMMain:testXML10QName:: $INPUT = $INPUT>) {
        __EXCEPTION{MDOMX:MDOM_NS_MALFORMED_QNAME::
          DOMCore:name => {$INPUT},
          MDOMX:param-name => '$INPUT_NAME',
          infoset:version => '1.0',
        }__;
