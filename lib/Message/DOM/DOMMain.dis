StringDataTypeDef:
  @QName: ManakaiDOMXML11QName
  @rdfs:subClassOf: ManakaiDOMXMLQName
  @enDesc:
    XML 1.1 Namespace Qualified Name.
  @enLabel:
    XML 1.1 QName
  
  @ResourceDef:
