Module:
  @Name:  DOMHTML
  @Namespace:
     http://suika.fam.cx/~wakaba/archive/2004/dom/html#
  @BindingName:
     @@@:  html
     @@Type:
        lang:IDL-DOM
     @@prefix:  dom.w3c.org
     @@Condition:DOM1
  @BindingName:
     @@@:  html2
     @@Type:
        lang:IDL-DOM
     @@prefix:  dom.w3c.org
     @@Condition:DOM2
  @FullName:
    @@lang:en
    @@@: DOM HTML Module
  @Description:
    @@lang:en
    @@@:
      The DOM HTML Module provides objects and methods specific 
      to HTML 4 documents and XHTML 1.0 documents. THe HTML 
      DOM exposes a number of convinience methods and attributes 
      that are more appropriate to script writers.  These 
      enhancements rely on transitional or frameset DTDs 
      of HTML 4.0 <SRC:DOM Level 1 HTML> or HTML 4.01 
      <SRC:DOM Level 2 HTML> and XHTML 1.0 <SRC:DOM Level 2 HTML>.

  @Author:
     @@FullName: Wakaba
     @@Mail:  w@suika.fam.cx
  @License:
     license:Perl+MPL
  @Date.RCS:
     $Date: 2004/10/18 12:23:51 $

  @ConditionDef:
    @@Name:DOM1
  @ConditionDef:
    @@Name:DOM2
    @@ISA:DOM1
  @NormalCondition:DOM2
  
  @Feature:
    @@Name:HTML
    @@Version:1.0
    @@FullName: DOM Level 1 HTML
    @@Condition:DOM1
    @@ImplNote:
      @@@lang:en
      @@@@:
        The DOM Level 2 HTML Specification does not say DOM Level 2 
        HTML implementations to answer to <M:hasFeature> of 
        <FeatureVer:1.0> as <DOM:true>.
    @@ImplNote:
      @@@lang:en
      @@@@:
        The DOM implementation must support 
        <Module:DOMCore> Module.
    @@Contrib:
      DOMCore:DOMImplementation::DOM1
    @@Contrib:
      DOMCore:DOMImplementation::DOM2
    @@Contrib:
      DOMCore:DOMImplementation::DOM3
  @Feature:
    @@Name:HTML
    @@Version:2.0
    @@FullName: DOM Level 2 HTML
    @@Condition:DOM2
    @@ImplNote:
      @@@lang:en
      @@@@:
        The DOM implementation must support 
        <Feature:Core>/<FeatureVer:2.0>.
    @@Contrib:
      DOMCore:DOMImplementation::DOM2
    @@Contrib:
      DOMCore:DOMImplementation::DOM3
  @Feature:
    @@Name:XHTML
    @@Version:2.0
    @@FullName: DOM Level 2 HTML (with XHTML support)
    @@Condition:DOM2
    @@ImplNote:
      @@@lang:en
      @@@@:
        Equal to <Feature:XML>/<FeatureVer:2.0> and  
        <Feature:HTML>/<FeatureVer:2.0>.
    @@Contrib:
      DOMCore:DOMImplementation::DOM2
    @@Contrib:
      DOMCore:DOMImplementation::DOM3

  @Require:
     @@Module:
        @@@Name:  DOMCore
        @@@FileName:
           @@@@@:  dom.idl
           @@@@For:
              lang:IDL-DOM
        @@@Namespace:
          http://suika.fam.cx/~wakaba/archive/2004/8/18/dom-core#
     @@Module:
        @@@Name:  DOMWebForms
        @@@Namespace:
          http://suika.fam.cx/~wakaba/archive/2004/dom/html#

