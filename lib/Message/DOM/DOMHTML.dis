             @@@@@@:
               The link is enabled.
        @@@Def:
          @@@@Type:
            lang:dis
          @@@@GetProp:
            DOMHTML:disabled
     @@Set:
        @@@Type:
           DOMMain:boolean
        @@@InCase:
           @@@@Value:  true
           @@@@Description:
             @@@@@lang:en
             @@@@@@:
               The link is disabled.
        @@@InCase:
           @@@@Value:  false
           @@@@Description:
             @@@@@lang:en
             @@@@@@:
               The link is enabled.
        @@@Def:
