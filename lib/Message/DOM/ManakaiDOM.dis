      @@@Type: unsigned-long
      @@@Value: 2
      @@@Description:
        @@@@lang:en
        @@@@@: 
          The specified index points between the high surrogate and
          the low surrogate.
      @@@Param:
        @@@@QName:
          ManakaiDOMImplementationException:param-name
        @@@@Description:
          @@@@@lang:en
          @@@@@@: The parameter to which inappropriate value is specified.

    @@Const:
