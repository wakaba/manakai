## -- Vocabulary Types

VocabTypeDef:
  @QName: DocumentType
  @instance: Document
  @definedBy: DocumentTypeDefinition
  @mayHave: ElementType

VocabTypeDef:
  @QName: ElementType
  @instance: Element
  @definedBy: ElementTypeDefinition
  @mayHave: AttributeType
  @mayHave: ContentType

VocabTypeDef:
  @QName: AttributeType
  @instance: Attribute
  @definedBy: AttributeTypeDefinition
  @mayHave: SimpleContentType

VocabTypeDef:
  @QName: ContentType
  @instance: Content
  @definedBy: ContentTypeDefinition

VocabTypeDef:
  @QName: SimpleContentType
  @rdfs:subClassOf: ContentType
  @instance: SimpleContent
  @definedBy: SimpleContentTypeDefinition

## -- Vocabulary Definition Constructions

VocabDefTypeDef:
  @QName: DocumentTypeDefinition
  @defines: DocumentType
  @mayHave: DocumentTypeDefinitionModule
  @mayHave: DocumentTypeDefinitionDriver

VocabDefTypeDef:
  @QName: DocumentTypeDefinitionModule
  @mayHave: DocumentTypeDefinitionModule
  @mayHave: ElementTypeDefinition
  @mayHave: ElementTypeDefinitionAttributeAddition
  @mayHave: ElementTypeDefinitionContentModification
  @mayHave: AttributeTypeDefinition
  @mayHave: AttributeTypeDefinitionSet
  @mayHave: AttributeTypeDefinitionSetAddition
  @mayHave: ContentTypeDefinition
  @mayHave: ElementTypeClassDefinition
  @mayHave: ElementTypeClassDefinitionAddition

VocabDefTypeDef:
  @QName: ElementTypeDefinition
  @defines: ElementType
  @mayHave: AttributeTypeDefinition
  @mayHave: AttributeTypeDefinitionSet
  @mayHave: ContentTypeDefinition

VocabDefTypeDef:
  @QName: ElementTypeDefinitionAttributeAddition
  @mayHave: AttributeTypeDefinition
  @mayHave: AttributeTypeDefinitionSet

VocabDefTypeDef:
  @QName: ElementTypeDefinitionContentModification
  @mayHave: ContentTypeDefinition

VocabDefTypeDef:
  @QName: AttributeTypeDefinition
  @defines: AttributeType
  @mayHave: SimpleContentTypeDefinition

VocabDefTypeDef:
  @QName: AttributeTypeDefinitionSet
  @mayHave: AttributeTypeDefinition
  @mayHave: AttributeTypeDefinitionSet

VocabDefTypeDef:
  @QName: AttributeTypeDefinitionSetAddition
  @mayHave: AttributeTypeDefinition
  @mayHave: AttributeTypeDefinitionSet

VocabDefTypeDef:
  @QName: ContentTypeDefinition
  @defines: ContentType

VocabDefTypeDef:
  @QName: SimpleContentTypeDefinition
  @defines: SimpleContentType
  @rdfs:subClassOf: ContentTypeDefinition

VocabDefTypeDef:
  @QName: ElementTypeClassDefinition
  @mayHave: ElementTypeClassDefinition
  @mayHave: ElementTypeDefinition

VocabDefTypeDef:
  @QName: ElementTypeClassDefinitionAddition
  @mayHave: ElementTypeClassDefinition
  @mayHave: ElementTypeDefinition

VocabDefTypeDef:
  @QName: DocumentTypeDefinitionDriver
  @mayHave: DocumentTypeDefinitionModule

@@@
  

## -- Markup Constructions

VocabTypeInstanceDef:
  @QName: Document
  @instanceOf: DocumentType

VocabTypeInstanceDef:
  @QName: Element
  @instanceOf: ElementType

VocabTypeInstanceDef:
  @QName: Attribute
  @instanceOf: AttributeType

VocabTypeInstanceDef:
  @QName: Content
  @instanceOf: ContentType

PropDef:
  @QName: instance

PropDef:
  @QName: instanceOf
