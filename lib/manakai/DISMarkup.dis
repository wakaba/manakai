## -- Markup language vocabulary types

ResourceTypeDef:
  @QName: mv|Item
  @enFN: item
  @enDesc:
    An <Q::mv|Item> is a component in markup language vocabulary.

ResourceTypeDef:
  @QName: mv|ItemType
  @enFN: item type
  @enDesc:
    An <Q::mv|ItemType> is a class of components in markup
    language vocabulary.
  @subsetOf: mv|Item

RPropDef:
  @QName: mv|name
  @enFN: name
  @enDesc:
    A <Q::mv|name> is a name to identity the <Q::mv|Item>.
  @subsetOf: dis|AppName
  @dataType: DISCore|String
  @multipleProperties: DISCore|UnorderedList

RPropDef:
  @QName: mv|id
  @enFN: identifier
  @enDesc:
    An <Q::mv|id> is a name of the <Q::mv|Item>, typically
    used in entire or a part of a name of a concrete
    representation of the <Q::mv|Item> or file name of it,
    without any <Q::mv|vid>.
  @subsetOf: mv|name
  @dataType: DISCore|String
  @multipleProperties: DISCore|Single

RPropDef:
  @QName: mv|vid
  @enFN: version identifier
  @enDesc:
    An <Q::mv|vid> is a version of the <Q::mv|Item>, typically
    used as a part of a name of a concreate representation
    of the <Q::mv|Item> or file name of it.
  @subsetOf: dis|AppName
  @dataType: DISCore|String
  @multipleProperties: DISCore|Single

RPropDef:
  @QName: mv|longName
  @enFN: long name
  @enDesc:
    A human readable name of the <Q::mv|Item>, without any <Q::mv|version>.
  @subsetOf: dis|AppName
  @dataType: DISCore|String
  @multipleProperties: DISCore|UnorderedList

RPropDef:
  @QName: mv|version
  @enFN: version
  @enDesc:
    A human readable version name of the <Q::mv|Item>.
  @subsetOf: dis|AppName
  @dataType: DISCore|String
  @multipleProperties: DISCore|UnorderedList

RPropDef:
  @QName: mv|shortDescription
  @enFN: short description
  @subsetOf: dis|Description
  @dataType: DISCore|String
  @multipleProperties: DISCore|Single

RPropDef:
  @QName: mv|targetNamespace
  @enFN: target namespace
  @enDesc:
    The namespace whose members are defined by the <Q::mv|Item>.
  @dataType: DISCore|QName
  @multipleProperties: DISCore|Single

RPropDef:
  @QName: mv|qualifiedName
  @enFN: qualified name
  @enDesc:
    The pair of the namespace URI and local name of the <Q::mv|Item>.
  @rdfs:domain: mv|Item
  @dataType: DISCore|QName
  @multipleProperties: DISCore|UnorderedList

RPropDef:
  @QName: mv|publicIdentifier
  @enFN: public identifier
  @enDesc:
    The public identifier of the <Q::mv|Item>.
  @rdfs:domain: mv|Item
  @dataType: DISCore|String
  @multipleProperties: DISCore|UnorderedList
  @subsetOf: mv|name

RPropDef:
  @QName: mv|systemIdentifier
  @enFN: system identifier
  @enDesc:
    The system identifier of the <Q::mv|Item>.
  @rdfs:domain: mv|Item
  @dataType: DISCore|String
  @multipleProperties: DISCore|UnorderedList
  @subsetOf: mv|name

RPropDef:
  @QName: mv|contains
  @enFN: contains
  @dataType: DISCore|TFQNames
  @multipleProperties: DISCore|OrderedList

ResourceTypeDef:
  @QName: mv|Document
  @enFN: document
  @enDesc:
    A <Q::mv|Document> is a document.
  @subsetOf: mv|Item

ResourceTypeDef:
  @QName: mv|DocumentType
  @enFN: document type
  @enDesc:
    A <Q::mv|DocumentType> is a class of documents.
  @subsetOf: mv|ItemType

RPropDef:
  @QName: mv|documentTypeName
  @enFN: document type name
  @enDesc:
    The name of the document type.
  @subsetOf: mv|name
  @dataType: DISCore|String
  @multipleProperties: DISCore|Single

ResourceTypeDef:
  @QName: mv|DocumentTypeDefinition
  @enFN: document type definition
  @enDesc:
    A <Q::mv|DocumentTypeDefinition> is a grammer for
    a <Q::mv|DocumentType>.
  @subsetOf: mv|Item

ResourceTypeDef:
  @QName: mv|DocumentTypeAnyModule
  @enFN: document type module or driver
  @subsetOf: mv|Item

ResourceTypeDef:
  @QName: mv|DocumentTypeModule
  @enFN: document type module
  @enDesc:
    A <Q::mv|DocumentTypeModule> is a module 
    that is a possible part of a <Q::mv|DocumentType>.
  @subsetOf: mv|DocumentTypeAnyModule

ResourceTypeDef:
  @QName: mv|DocumentTypeDriver
  @enFN: document type driver
  @subsetOf: mv|DocumentTypeAnyModule

ResourceTypeDef:
  @QName: mv|DocumentTypeModuleSet
  @enFN: document type module set
  @enDesc:
    A <Q::mv|DocumentTypeModuleSet> is a set of zero or more
    <Q::mv|DocumentTypeModule>s.
  @subsetOf: mv|Item

ResourceTypeDef:
  @QName: mv|Element
  @enFN: element
  @enDesc:
    An <Q::mv|Element> is an element in markup language vocabulary.
  @subsetOf: mv|Item

ResourceTypeDef:
  @QName: mv|ElementType
  @enFN: element type
  @enDesc:
    An <Q::mv|ElementType> is a class of <Q::mv|Element>s.
  @subsetOf: mv|ItemType
  @subsetOf: mv|ElementTypeOrNSElementTypes

RPropDef:
  @QName: mv|contentModel
  @enFN: content model

ResourceTypeDef:
  @QName: mv|ElementTypeClass
  @enFN: element type class
  @subsetOf: mv|ElementType

ResourceTypeDef:
  @QName: mv|ElementTypeMix
  @enFN: element type mixed class
  @subsetOf: mv|ElementType

RPropDef:
  @QName: mv|elementTypeName
  @enFN: element type name
  @enDesc:
    The name of the element type.
  @subsetOf: mv|name
  @rdfs:domain: mv|ElementtypeOrNSElementTypes
  @dataType: DISCore|QName
  @multipleProperties: DISCore|Single

ResourceTypeDef:
  @QName: mv|ElementTypes
  @enFN: element types
  @enDesc:
    An <Q::mv|ElementTypes> is a set of <Q::mv|ElementType>s.

ResourceTypeDef:
  @QName: mv|NSElementTypes
  @enFN: element types in a namespace
  @enDesc:
    An <Q::mv|NSElementTypes> is a set of <Q::mv|ElementType>s
    belong to a <Q::mv|Namespace>.
  @subsetOf: mv|ElementTypes
  @subsetOf: mv|ElementTypeOrNSElementTypes

ResourceTypeDef:
  @QName: mv|ElementTypeOrNSElementTypes
  @enFN: element type or element types in a namespace
  @enDesc:
    An <Q::mv|ElementTypeOrNSElementTypes> is either
    a <Q::mv|ElementType> or a <Q::mv|NSElementTypes>.

ElementTypeBinding:
  @Name: ResourceTypeDef
  @ElementType: 
    dis:ResourceDef
  @ShadowContent:
    @@DISCore:resourceType: DISCore|resourceType
    @@subsetOf: DISCore|resourceType

ElementTypeBinding:
  @Name: enDesc
  @ElementType:
    dis:Description
  @ShadowContent:
    @@lang:en

ElementTypeBinding:
  @Name: enFN
  @ElementType:
    dis:FullName
  @ShadowContent:
    @@lang:en

ResourceTypeDef:
  @QName: ElementTypeOrElementInNS
  @AliasFor: mv|ElementTypeOrNSElementTypes

ResourceTypeDef:
  @QName: ElementType
  @AliasFor: mv|ElementType

ResourceTypeDef:
  @QName: AnyElementInNS
  @AliasFor: mv|NSElementTypes

ResourceTypeDef:
  @QName: mv|ElementContentModel
  @enFN: element content model
  @enDesc:
    An <Q::mv|ElementContentModel> is a content model for
    element types.
  @subsetOf: mv|Item
  @subsetOf: DISCore|AnyType

RPropDef:
  @QName: mv|elementContentModel
  @enFN: element content model
  @enDesc:
    The <Q::mv|elementContentModel> applied to an <Q::mv|ElementType>.
  @dataType: DISCore|TFQNames
  @multipleProperties: DISCore|Single

RPropDef:
  @QName: mv|elementContentModelName
  @enFN: element content model name
  @subsetOf: mv|name
  @dataType: DISCore|String
  @multipleProperties: DISCore|Single

ResourceTypeDef:
  @QName: mv|Attribute
  @enFN: attribute
  @enDesc:
    An <Q::mv|Attribute> is an attribute in markup language
    vocabulary.
  @subsetOf: mv|Item

ResourceTypeDef:
  @QName: Attribute
  @AliasFor: mv|Attribute

ResourceTypeDef:
  @QName: mv|AttributeType
  @enFN: attribute type
  @enDesc:
    An <Q::mv|AttributeType> is a class of attributes.
  @subsetOf: mv|ItemType

RPropDef:
  @QName: mv|attributeTypeName
  @enFN: attribute type name
  @enDesc:
    The name of the <Q::mv|AttributeType>.
  @rdfs:domain: mv|AttributeType
  @subsetOf: mv|name
  @dataType: DISCore|QName
  @multipleProperties: DISCore|Single

RPropDef:
  @QName: mv|attributeValueType
  @enFN: attribute value type
  @dataType: DISCore|TFQNames
  @multipleProperties: DISCore|Single

ResourceTypeDef:
  @QName: mv|AttributeTypeGroup
  @enFN: attribute type group
  @subsetOf: mv|Item

RPropDef:
  @QName: mv|attributeTypeGroupName
  @enFN: attribute type group name
  @subsetOf: mv|name
  @dataType: DISCore|String
  @multipleProperties: DISCore|Single

ResourceTypeDef:
  @QName: AnyAttributeInNS
  @enDesc:
    Attribute name wildcards that represent any attribute
    in a particular namespace.

ResourceTypeDef:
  @QName: mv|SimpleType
  @enFN: simple type
  @enDesc:
    A <Q::mv|SimpleType> is a set of constaraints to strings.
    It is generally defines its semantics as well as syntax.
  @subsetOf: mv|Item
  @subsetOf: DISCore|AnyType

RPropDef:
  @QName: mv|simpleTypeName
  @enFN: simple type name
  @subsetOf: mv|name
  @dataType: DISCore|String
  @multipleProperties: DISCore|Single

ResourceTypeDef:
  @QName: mv|AttributeValueType
  @enFN: attribute value type
  @subsetOf: mv|SimpleType

ResourceTypeDef:
  @QName: mv|Notation
  @enFN: notation
  @enDesc:
    A <Q::mv|Notation> is a notation in XML.
  @subsetOf: mv|Item
  @subsetOf: DISCore|AnyType

## -- Markup language vocabulary characteristics

ElementTypeBinding:
  @Name: RPropDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@DISCore:resourceType: DISSource|ResourceProperty

RPropDef:
  @QName: mv|elementType
  @enFN: associated element type
  @enDesc:
    If an <VAR::S>'s <Q::mv|ElementType> is <VAR::O>,
    <VAR::S> is associated to element type <VAR::O>.
  @rdfs:range: mv|ElementTypeOrNSElementTypes
  @dataType: DISCore|TFQNames
  @multipleProperties: DISCore|UnorderedList

ResourceDef:
  @QName: elementType
  @AliasFor: mv|elementType

RPropDef:
  @QName: mv|rootElementType
  @enFN: associated root element type
  @enDesc:
    If an <VAR::S>'s is <Q::mv|rootElementType>, 
    <VAR::S> is associated to documents whose root element
    type is <VAR::O>.
  @rdfs:range: mv|ElementTypeOrNSElementTypes
  @dataType: DISCore|TFQNames
  @multipleProperties: DISCore|UnorderedList

ResourceDef:
  @QName: rootElementType
  @AliasFor: mv|rootElementType

RPropDef:
  @QName: mv|documentType
  @enFN: associated document type
  @enDesc:
    If an <VAR::S>'s <Q::mv|documentType> is <VAR::O>,
    <VAR::S> is associated to documents whose document
    type is <VAR::O>.

RPropDef:
  @QName: mv|attributeType
  @enFN: associated attribute
  @enDesc:
    If an <VAR::S>'s <Q::mv|attribute> is <VAR::O>, 
    <VAR::S> is associated to attribute <VAR::O>.
  @rdfs:range: mv|AttributeType
  @dataType: DISCore|TFQNames
  @multipleProperties: DISCore|UnorderedList

RPropDef:
  @QName: dis|DocAttr
  @AliasFor: mv|attributeType

## -- XML DTD Modules

ResourceTypeDef:
  @QName: mv|XMLDTDModuleSet
  @enFN: XML DTD module set
  @enDesc:
    A set of <Q::mv|XMLDTDModule>s.
  @subsetOf: mv|DocumentTypeModuleSet

ResourceTypeDef:
  @QName: mv|XMLDTDModule
  @enFN: XML DTD module
  @enDesc:
    An <Q::mv|XMLDTDModule> is an XML DTD module.
  @subsetOf: mv|DocumentTypeModule

ResourceTypeDef:
  @QName: mv|XMLDTDDatatypeEntity
  @enFN: XML DTD datatype parameter entity
  @subsetOf: mv|SimpleType
  @subsetOf: dxm|ParameterEntity
  @subsetOf: dxm|InternalEntity

RPropDef:
  @QName: mv|datatypeEntityName
  @enFN: datatype entity name
  @enDesc:
    The name of the <Q::mv|DatatypeEntity>, without <XML::.datatype>
    suffix and possible prefix.
  @dataType: DISCore|String
  @multipleProperties: DISCore|Single

RPropDef:
  @QName: mv|datatypeXMLAttributeType
  @enFN: XML attribute type
  @dataType: DISCore|QName
  @multipleProperties: DISCore|Single

## -- "For markup" definitions

ForDef:
  @QName:
    s:ForML
  @FullName:
    @@lang:en
    @@@:
      For a markup language

ForDef:
  @QName:
    s:ForET
  @FullName:
    @@lang:en
    @@@:
      For an element type

ForDef:
  @QName: mv|ForCM
  @enFN: for content model
  @ISA: s|ForML

ForDef:
  @QName: mv|ForAG
  @enFN: for attribute type group
  @ISA: s|ForML

