## -- Markup language vocabulary types

ElementTypeBinding:
  @Name: ResourceTypeDef
  @ElementType: 
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type:
      rdfs:Class
    @@ISA:
      rdfs:Class
    @@ForCheck:
      =ManakaiDOM:all

ElementTypeBinding:
  @Name: enDesc
  @ElementType:
    dis:Description
  @ShadowContent:
    @@lang:en

ResourceTypeDef:
  @QName: ElementTypeOrElementInNS
  @enDesc:
    <Q::ElementType>s and <Q::AnyElementInNS>'es.

ResourceTypeDef:
  @QName: ElementType
  @Description:
    @@lang:en
    @@@:
      A markup language element type. 
  @rdfs:subClassOf: ElementTypeOrElementInNS

ResourceTypeDef:
  @QName: AnyElementInNS
  @enDesc:
    Element type wildcards that represent any element type
    in a particular namespace.
  @rdfs:subClassOf: ElementTypeOrElementInNS

ResourceTypeDef:
  @QName: Attribute
  @Description:
    @@lang:en
    @@@:
      A markup language element attribute type.  

ResourceTypeDef:
  @QName: AnyAttributeInNS
  @enDesc:
    Attribute name wildcards that represent any attribute
    in a particular namespace.

## -- Markup language vocabulary characteristics

ElementTypeBinding:
  @Name: PropDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type:
      rdf:Property
    @@AliasFor:
      @@@@:
        ::ManakaiDOM:all
      @@@For:
        !=ManakaiDOM:all

PropDef:
  @QName: elementType
  @Description:
    @@lang:en
    @@@:
      A subject resource is associated with an object element type. 
  @rdfs:domain: AnyElementInNS
  @dataType:
    dis:TFQNames

PropDef:
  @QName: rootElementType
  @enDesc:
    The subject resource is associated with documents whose
    root element type is identified by the object resource.
  @rdfs:domain: AnyElementInNS
  @dataType:
    dis:TFQNames

PropDef:
  @QName:
    dis:DocAttr
  @Description:
    @@lang:en
    @@@:
      A subject resource is associated with an object markup attribute. 
  @rdfs:domain: Attribute
  @dataType:
    dis:TFQNames

## -- "For markup" definitions

ForDef:
  @QName:
    s:ForML
  @FullName:
    @@lang:en
    @@@:
      For a markup language

ForDef:
  @QName:
    s:ForET
  @FullName:
    @@lang:en
    @@@:
      For an element type

