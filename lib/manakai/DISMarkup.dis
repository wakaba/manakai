## -- Markup language vocabulary types

ElementTypeBinding:
  @Name: ResourceTypeDef
  @ElementType: 
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type:
      rdfs:Class
    @@ISA:
      rdfs:Class
    @@AliasFor:
      @@@@:
        ::ManakaiDOM:all
      @@@For:
        !=ManakaiDOM:all

ResourceTypeDef:
  @QName: ElementType
  @Description:
    @@lang:en
    @@@:
      A markup language element type. 

ResourceTypeDef:
  @QName: Attribute
  @Description:
    @@lang:en
    @@@:
      A markup language element attribute type.  

## -- Markup language vocabulary characteristics

ElementTypeBinding:
  @Name: PropDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type:
      rdfs:Property
    @@AliasFor:
      @@@@:
        ::ManakaiDOM:all
      @@@For:
        !=ManakaiDOM:all

PropDef:
  @QName: elementType
  @Description:
    @@lang:en
    @@@:
      A subject resource is associated with an object element type. 
  @rdfs:domain: ElementType
  @dataType:
    dis:TypeForQNames

PropDef:
  @QName:
    dis:DocAttr
  @Description:
    @@lang:en
    @@@:
      A subject resource is associated with an object markup attribute. 
  @rdfs:domain: Attribute
  @dataType:
    dis:TypeForQNames

## -- "For markup" definitions

ForDef:
  @QName:
    s:ForML
  @FullName:
    @@lang:en
    @@@:
      For a markup language

ForDef:
  @QName:
    s:ForET
  @FullName:
    @@lang:en
    @@@:
      For an element type

