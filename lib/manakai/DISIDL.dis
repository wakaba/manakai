Module:
  @QName: 
    DIS:DISIDL
  @FullName:
    @@lang:en
    @@@: 
      <QUOTE::dis> IDL Module
  @Namespace:
    http://suika.fam.cx/~wakaba/archive/2004/dis/IDL#

  @Description:
    @@lang:en
    @@@:
      This module defines IDL datatypes, as defined in 
      <CITE::OMG IDL Syntax and Semantics> in
      <CITE::The Common Object Request Broker: Architecture
             and Specification, version 2>,
      for the purpose of <QUOTE::dis> system. 
  @ImplNote:
    @@lang:en
    @@@:
      CORBA 2.0: <lt>http://www.omg.org/cgi-bin/doc?formal/97-02-25>
  
  @Author:
    @@FullName:Wakaba
    @@Mail:w@suika.fam.cx
  @License:
     license:Perl+MPL
  @Date:
    @@@:
      $Date: 2005/04/27 12:32:39 $
    @@ContentType:
      dis:Date.RCS
  
  @DefaultFor:
    ManakaiDOM:all

  @Require:
    @@Module:
      @@@Name: DISCore

Namespace:
  @DIS:
    http://suika.fam.cx/~wakaba/archive/2004/dis/
  @dis:
    http://suika.fam.cx/~wakaba/archive/2004/8/18/lang#dis--
  @DOMMain:
    http://suika.fam.cx/~wakaba/archive/2004/dom/main#
  @lang:
     http://suika.fam.cx/~wakaba/archive/2004/8/18/lang#
