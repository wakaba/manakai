## -- IDL datatypes

ResourceDef:
  @Name: IDLDataType
  @enFN: IDL data type
  @Description:
    @@lang:en
    @@@:
      A CORBA IDL data type.
  @rdf:type:
    rdfs:Class
  @subsetOf: DISLang|DataType
  @rdfs:subClassOf: DISLang|DataType

ElementTypeBinding:
  @Name: IDLDataTypeDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type: IDLDataType

IDLDataTypeDef:
  @QName: idl|void
  @enFN:
    IDL <CODE::void> Type
  @idl:java: j|void

ResourceDef:
  @QName: AnyRealNumber
  @enFN: Real number
  @Description:
    @@lang:en
    @@@:
      Any IDL datatype that represents a subset of the real numbers.
  @rdf:type: DISLang|DataType
  @idl:perl: p|Number

IDLDataTypeDef:
  @QName: idl|long
  @enFN: Long integer
  @Description:
    @@lang:en
    @@@:
      Long signed integer representing the range -2^31 ... 2^31 - 1. 
  @rdfs:subClassOf: AnyRealNumber
  @subsetOf: DISIDL|AnyRealNumber
  @idl:java: j|int
  @idl:perl: p|Number
  @idl:ecmascript: js|NumberType

ResourceDef:
  @QName: DOMMain|long
  @AliasFor: idl|long

IDLDataTypeDef:
  @QName: idl|unsignedLong
  @enFN: Unsigned long integer
  @Description:
    @@lang:en
    @@@: 
      Long unsigned integer representing the range 0 ... 2^32 - 1. 

    {NOTE:: In DOM Java Binding, the IDL type <TYPE::idl:unsignedLong>,
            32-bit <EM::unsigned> integer, is bound to the Java
            type <TYPE::j:int>, 32-bit <EM::signed> integer.
    }
  @rdfs:subClassOf: AnyRealNumber
  @subsetOf: DISIDL|AnyRealNumber
  @idl:typeName: unsigned long
  @idl:java: j|int
  @idl:ecmascript: js|NumberType
  @idl:perl: p|Number

ResourceDef:
  @QName: DOMMain|unsigned-long
  @AliasFor: idl|unsignedLong

IDLDataTypeDef:
  @QName: idl|short
  @enFN: Short integer
  @Description:
    @@lang:en
    @@@: 
      Short signed integer representing the range -2^15 ... 2^15 - 1.
  @rdfs:subClassOf: AnyRealNumber
  @subsetOf: DISIDL|AnyRealNumber
  @idl:java: j|short
  @idl:perl: p|Number
  @idl:ecmascript: js|NumberType

ResourceDef:
  @QName: DOMMain|short
  @AliasFor: idl|short

IDLDataTypeDef:
  @QName: idl|unsignedShort
  @enFN: Unsigned short integer
  @idl:typeName: unsigned short
  @Description:
    @@lang:en
    @@@: 
      Short unsigned integer representing the range 0 ... 2^16 - 1.
  @rdfs:subClassOf: AnyRealNumber
  @subsetOf: DISIDL|AnyRealNumber
  @idl:perl: p|Number

ResourceDef:
  @QName: DOMMain|unsigned-short
  @AliasFor: idl|unsignedShort

IDLDataTypeDef:
  @QName: idl|float
  @enFN:
    Single-precision floating point number
  @Description:
    @@lang:en
    @@@:
      IEEE signle-precision floating point numbers 
      <SRC::ANSI/IEEE Std 754-1985>. 
      \
      {NOTE:: The CORBA specification sais implementations that 
              do not fully support the value set of the IEEE 754 
              floating-point standard to completely specify 
              their deviance from the standard. 
      \
      }
  @rdfs:subClassOf: AnyRealNumber
  @subsetOf: DISIDL|AnyRealNumber
  @idl:java: j|float
  @idl:perl: p|Number
  @idl:ecmascript: js|NumberType

ResourceDef:
  @QName: DOMMain|float
  @AliasFor: idl|float

IDLDataTypeDef:
  @QName: idl|double
  @enFN: Double-precision floating point number
  @Description:
    @@lang:en
    @@@:
      IEEE double-precision floating point numbers 
      <SRC::ANSI/IEEE Std 754-1985>. 
      \
      {NOTE:: The CORBA specification sais implementations that
              do not fully support the value set of the IEEE 754
              floating-point standard to completely specify
              their deviance from the standard.
      \
      }
  @rdfs:subClassOf: AnyRealNumber
  @subsetOf: DISIDL|AnyRealNumber
  @idl:java: j|double
  @idl:perl: p|Number
  @idl:ecmascript: js|NumberType

ResourceDef:
  @QName: DOMMain|double
  @AliasFor: idl|double

IDLDataTypeDef:
  @QName: idl|char
  @enFN: ISO/IEC 8859-1 character
  @Description:
    @@lang:en
    @@@:
      ISO/IEC 8859-1 characters.  During transmission, characters 
      may be converted to other appropriate forms as required 
      by a particular language binding.  Such convertions may 
      change the representation of a character but maintain 
      the semantics of the character. 

IDLDataTypeDef:
  @QName: idl|boolean
  @enFN: Boolean
  @Description:
    @@lang:en
    @@@:
      The boolean data type, i.e. true or false. 
  @idl:java: j|boolean
  @idl:perl: p|Boolean
  @idl:ecmascript: js|BooleanType

ResourceDef:
  @QName: DOMMain|boolean
  @AliasFor: idl|boolean

IDLDataTypeDef:
  @QName: idl|octet
  @enFN: Octet
  @Description:
    @@lang:en
    @@@:
      An 8-bit quantity that is guaranteed not to undergo any conversion 
      when transmitted by the communication system. 

IDLDataTypeDef:
  @QName: idl|any
  @enFN: Any IDL type
  @Description:
    @@lang:en
    @@@:
      This type permits the specification of values that can 
      express any OMG IDL type. 
  @rdfs:subClassOf: DISLang|Any
  @subsetOf: DISLang|Any
  @idl:java: jlang|Object
  @idl:perl: p|Any
  @idl:ecmascript: js|AnyType

ResourceDef:
  @QName: DOMMain|any
  @AliasFor: idl|any

IDLDataTypeDef: 
  @QName: idl|Object
  @enFN: Object
  @Description:
    @@lang:en
    @@@: An object.
  @idl:java: jlang|Object
  @idl:perl: p|Object
  @idl:ecmascript: js|Object

ResourceDef:
  @QName: DOMMain|Object
  @AliasFor: idl|Object

IDLDataTypeDef:
  @QName: idl|unsignedLongLong
  @enFN: Unsigned long long integer
  @idl:typeName: unsigned long long
  @Label:
    @@lang:en
    @@@: unsigned long long
  @FullName:
    @@lang:en
    @@@: Unsigned long long integer
  @rdfs:subClassOf: AnyRealNumber
  @subsetOf: DISIDL|AnyRealNumber
  @idl:java: jlang|long
  @idl:perl: p|Number

ResourceDef:
  @QName: DOMMain|unsigned-long-long
  @AliasFor: idl|unsignedLongLong

