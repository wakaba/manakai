  @enDesc:
    The effective structure and constraints of a given document type.
    The document model constitutes the abstract representation of 
    the physical or semantic structures of a class of documents.
  @subsetOf: mv|DocumentType

ResourceTypeDef:
  @QName:
    m12n:DocumentType
