InfoPropDef:
  @QName:
    infoset:attributes
  @InfosetName: attributes
  @DISPerl:propHashKey: attr
  @enDesc:
    An unordered set of attribute information items,
    corresponding to the attributes of this element,
    either directly specified or defaulted from the DTD. 
    Namespace declaration attributes do not appear in this set.
  @rdfs:domain:
    infoset:Element
  @rdfs:range:
    infoset:AttributeSet

InfoPropDef:
  @InfosetName: namespace attributes
  @enDesc:
    An unordered set of attribute information items,
    corresponding to the attributes of this element,
    either directly specified or defaulted from the DTD.
  @rdfs:domain:
    infoset:Element
  @rdfs:range:
    infoset:AttributeSet

InfoPropDef:
  @QName:
    infoset:inScopeNamespaces
  @InfosetName: in-scope namespaces
  @enDesc:
    An unordered set of namespace information items in effect for 
    this element information item.  
    \
    This set always contains a namespace information item
    with the <InfoProp::prefix> of <XML::xml> that is implicitly 
    bound to the <InfoProp::namespace name> 
    <URI::http://www.w3.org/XML/1998/namespace>.
    It does not contain a namespace information item
    with prefix <XML::xmlns>.
    \
    This set will include namespace information items corresponding 
    to all of the members of <InfoProp::namespace attributes>
    except for any declarations that undeclare prefixes.
  @rdfs:domain:
    infoset:Element
  @rdfs:range:
    infoset:NamespaceSet

ResourceDef:
  @QName:
    infoset:NamespaceSet
  @rdf:type:
    rdfs:Class
  @rdfs:subClassOf:
    infoset:InfoItemSet

InfoPropDef:
  @QName:
    infoset:parent
  @InfosetName: parent
  @DISPerl:propHashKey: parent
  @enDesc:
    <InfoItem::Element information items>::: The 
    <InfoItem::document information item> or 
    <InfoItem::element information item> that contains 
    this information item in its <InfoProp::children> property.
    \
    <InfoItem::Processing instruction information items>::: The 
        <InfoItem::document information item>, 
        <InfoItem::element information item> or
        <InfoItem::document type declaration information item>
        that contains this information item in its <InfoProp::children>
        property.
    \
    <InfoItem::Unexpanded entity reference information items>:::
        The <InfoItem::element information item> that
        contains this information item in its <InfoProp::children> property.
    \
    <InfoItem::Character information items>:::
        The <InfoItem::element information item> that
        contains this information item in its <InfoProp::children> property.
    \
    - <InfoItem::Comment information items>:::
        The <InfoItem::document information item> or
        <InfoItem::element information item> that
        contains this information item in its <InfoProp::children> property.
    \
    - <InfoItem::Document type declaration items>:::
        The <InfoItem::document information item>.
  @rdfs:domain:
    infoset:InfoItem
  @rdfs:range:
    infoset:InfoItem

