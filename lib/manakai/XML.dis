    Note that the XML declaration and text declarations are not
    considered as processing instructions.
  @is:hasProperty:
    infoset:target
  @is:hasProperty:
    infoset:content
  @is:hasProperty:
    infoset:baseURI
  @is:hasProperty:
    infoset:notation
  @is:hasProperty:
    infoset:parent
  @is:hasProperty: includeHistory

InfoPropDef:
  @QName:
    infoset:target
