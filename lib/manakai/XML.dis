    @@@:
      SGML system identifier
  @rdfs:subClassOf: ExternalID

StringDataTypeDef:
  @QName: 
    DOMMain:XMLSystemID
  @FullName:
    @@lang:en
    @@@:
      XML system identifier
  @rdfs:subClassOf: 
    DOMMain:SystemID

StringDataTypeDef:
  @QName: 
    DOMMain:PublicID
  @FullName:
    @@lang:en
    @@@:
      Web SGML public identifier
  @rdfs:subClassOf:
    ExternalID

StringDataTypeDef:
  @QName: 
    DOMMain:SGMLPublicID
  @FullName:
    @@lang:en
    @@@:
      Classic SGML public identifier
  @rdfs:subClassOf: 
    DOMMain:PublicID

StringDataTypeDef:
  @QName: 
    DOMMain:XMLPublicID
  @FullName:
    @@lang:en
    @@@:
      XML public identifier
  @rdfs:subClassOf: 
    DOMMain:PublicID

