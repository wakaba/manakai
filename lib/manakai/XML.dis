InfoItemDef:
  @QName:
    infoset:Document
  @InfosetName: document
  @dxm:nodeTypeName: d
  @enDesc:
    There is exactly one document information item in the information
    set for an XML document and all other information items are
    accessible from the properties of the document information item,
    either directly or indirectly.
  @is:hasProperty: 
    infoset:children
  @is:hasProperty: 
    infoset:documentElement
  @is:hasProperty: 
    infoset:notations
  @is:hasProperty: 
    infoset:unparsedEntities
  @is:hasProperty: 
    infoset:baseURI
  @is:hasProperty:
    infoset:characterEncodingScheme
  @is:hasProperty:
    infoset:standalone
  @is:hasProperty:
    infoset:version
  @is:hasProperty:
    infoset:allDeclarationsProcessed

InfoPropDef:
  @QName:
    infoset:children
  @InfosetName: children
  @DISPerl:propHashKey: child
  @enDesc:
    An ordered list of child information items, in document order. 
    \
    - <InfoItem::Document information item>::: The list contains exactly one
        <InfoItem::element information item> and it may contain
        <InfoItem::processing instruction information items>,
        <InfoItem::comment information items> and/or
        a <InfoItem::document type declaration information item>.
    \
    - <InfoItem::Element information items>::: The list contains 
        <InfoItem::element information items>, 
        <InfoItem::processing instruction information items>,
        <InfoItem::unexpanded entity reference information items>,
        <InfoItem::character information items> and/or
        <InfoItem::comment information items>.
    \
    - <InfoItem::Document type declaration information item>:::
         The list contains <InfoItem::processing instruction items>
         appearing in the DTD, in the original document order. 
         Information items from the internal subset appear before
         those in the external subset.
  @rdfs:domain:
    infoset:InfoItem
  @rdfs:range:
    infoset:InfoItemSeq

InfoPropDef:
  @QName:
    infoset:documentElement
  @InfosetName: document element
  @enDesc:
    The element information item corresponding to the document element.
  @rdfs:domain:
