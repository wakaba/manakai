InfoPropDef:
  @QName:
    infoset:standalone
  @InfosetName: standalone
  @enDesc:
    An indication of the standalone status of the document.
  @InCase:
    @@Value:
      @@@@:
        infoset:Standalone
      @@@ContentType: DISCore|QName
    @@enDesc:
      The standalone declaration in the XML declaration of the 
      document entity is set to <XML::yes>.
  @InCase:
    @@Value:
      @@@@:
        infoset:Standalone.no
      @@@ContentType: DISCore|QName
    @@enDesc:
      The standalone declaration in the XML declaration of the
      document entity is set to <XML::false>.
  @noValueCase:
    @@enDesc:
      There is no standalone declaration in the document entity.
  @rdfs:domain:
    infoset:Document
  @rdfs:range:
    infoset:Standalone
  @Type:
    infoset:Standalone::ManakaiDOM:all

