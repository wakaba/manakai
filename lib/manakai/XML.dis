  @Type:
    infoset:Literal::ManakaiDOM:all

InfoPropDef:
  @QName:
    infoset:allDeclarationsProcessed
  @InfosetName: all declarations processed
  @enDesc:
    An indication of whether the XML processor has read the complete
    DTD or not.  This property is not strictly speaking part of the 
    information set of the document.
  @trueCase:
    @@enDesc:
      Properties are never unknown.
  @falseCase:
    @@enDesc:
      Certain properties may be unknown.
  @rdfs:domain: 
    infoset:Document
  @rdfs:range:
    infoset:Boolean
  @Type:
    infoset:Boolean::ManakaiDOM:all

