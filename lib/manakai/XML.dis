  @rdfs:domain:
    infoset:Character
  @rdfs:range:
    infoset:Integer

InfoPropDef:
  @QName:
    infoset:elementContentWhitespace
  @InfosetName: element content whitespace
  @DISPerl:propHashKey: ecws
  @enDesc:
    A boolean indicating whether the character is white space
    appearing within element content or not. 
    \
    {NOTE:: Validating XML processors are <EM::required> to
            provide this property.
    \
    }
  @trueCase:
