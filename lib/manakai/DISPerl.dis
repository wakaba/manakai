## -- Code fragment

ResourceDef:
  @QName: StringRef
  @Def:
    @@ContentType: lang|Perl
    @@@:
      if (defined $given) {
        if (ref $given eq 'SCALAR') {
          $result = $given;
        } else {
          require Scalar::Util;
          $result = ($Message::Util::ManakaiStringRef::Value{$given}
                     ||= \($given));
          Scalar::Util::weaken ($Message::Util::ManakaiStringRef::Value{$given});
        }
      } else {
        $result = null;
      }
  @rdf:type: DISPerl|BlockCode

ResourceDef:
  @QName: HashStringRef
  @Def:
    @@ContentType: lang|Perl
    @@@:
      if (defined $given) {
        if (ref $given eq 'SCALAR') {
          $result = $given;
        } elsif (defined $Message::Util::ManakaiStringRef::Value{$given}) {
          $result = $Message::Util::ManakaiStringRef::Value{$given};
        } else {
          require Scalar::Util;
          $result = $Message::Util::ManakaiStringRef::Value{$given} = \($given);
          Scalar::Util::weaken ($Message::Util::ManakaiStringRef::Value{$given});
        }
      } else {
        CORE::delete $result;
      }
  @rdf:type: DISPerl|BlockCode

