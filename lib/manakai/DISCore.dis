Module:
  @QName: 
    DIS:DISCore
  @FullName:
    @@lang:en
    @@@: 
      <QUOTE::dis> Format Core Module
  @Namespace:
    http://suika.fam.cx/~wakaba/archive/2004/dis/Core#

  @Description:
    @@lang:en
    @@@:
      This module introduces the basic <QUOTE::dis> language 
      vocabulary that is common to <QUOTE::dis> format. 
  
  @DISCore:author: DISCore|Wakaba
  @License:
     license:Perl+MPL
  @Date:
    @@@:
      $Date: 2005/11/13 04:45:09 $
    @@ContentType:
      dis:Date.RCS
  
  @DefaultFor:
    ManakaiDOM:all

  @Require:
    @@Module:
      @@@QName: DIS|DISRDF
    @@Module:
      @@@QName: DIS|DISPerl
    @@Module:
      @@@QName: DIS|Document

Namespace:
  @DIS:
    http://suika.fam.cx/~wakaba/archive/2004/dis/
  @dis:
    http://suika.fam.cx/~wakaba/archive/2004/8/18/lang#dis--
  @DISCore:
    http://suika.fam.cx/~wakaba/archive/2004/dis/Core#
  @kwd:
    http://suika.fam.cx/~wakaba/archive/2005/rfc2119/
  @lang:
     http://suika.fam.cx/~wakaba/archive/2004/8/18/lang#
  @license:
     http://suika.fam.cx/~wakaba/archive/2004/8/18/license#
  @ManakaiDOM:
    http://suika.fam.cx/~wakaba/archive/2004/8/18/manakai-dom#
  @owl:
    http://www.w3.org/2002/07/owl#
  @rdf:
    http://www.w3.org/1999/02/22-rdf-syntax-ns#
  @rdfs:
    http://www.w3.org/2000/01/rdf-schema#

## -- dis Resources

ResourceTypeDef:
  @QName: DISCore|AnyResource
  @enFN: 
    <QUOTE::dis> Any Kind of Resource
  @rdfs:subClassOf: rdfs|Resource
  @subsetOf: rdfs|Resource

## -- dis Module

ResourceTypeDef:
  @QName: DISCore|Module
  @enFN: 
    <QUOTE::dis> Module
  @rdfs:subClassOf: DISCore|AnyResource
  @subsetOf: DISCore|AnyResource
  @rdfs:subClassOf: DISCore|AnyModule
  @subsetOf: DISCore|AnyModule

ResourceTypeDef:
  @QName: DISCore|requires
  @enFN:
    Required <QUOTE::dis> Module
  @rdfs:domain: DISCore|Module
  @rdfs:range: DISCore|Module
  @dataType: DISCore|ModuleTFQNames
  @multipleProperties: DISCore|UnorderedList

## -- dis "for"

ResourceTypeDef:
  @QName: DISCore|For
  @enFN:
    <QUOTE::dis> <QUOTE::for>
  @enDesc:
    The target <EM::for> which resources are defined.
  @rdfs:subClassOf: DISCore|AnyResource
  @subsetOf: DISCore|AnyResource

CPropDef:
  @QName: DISCore|for
  @enFN: For
  @enDesc:
    The target for which the resource is defined.

    {NOTE:: For the <Q::DISCore:For> resources, the <Q::DISCore:for>
            resource is the <Q::ManakaiDOM:all> by definition.
    }
  @rdfs:domain: DISCore|AnyResource
  @rdfs:range: DISCore|For
  @multipleProperties: DISCore|Single

CPropDef:
  @QName: DISCore|forp
  @enFN: For (additional)
  @enDesc:
    Additional target specification.

    {NOTE:: The <Q::DISCore:For> resources have no <Q::DISCore:forp>
            resource.
    }
  @rdfs:domain: DISCore|AnyResource
  @rdfs:range: DISCore|For
  @multipleProperties: DISCore|OrderedList

SPropDef:
  @QName: dis|DefaultFor
  @Description:
    @@lang:en
    @@@:
      The default <QUOTE::for> for the module.
  @rdfs:domain: DISCore|Module
  @rdfs:range: DISCore|For
  @dataType: DISCore|ForQName
  @multipleProperties: DISCore|Single

ForDef:
  @QName:
    ManakaiDOM:all
  @FullName:
    @@lang:en
    @@@: For all

## -- dis Resources

ResourceTypeDef:
  @QName: DISCore|Resource
  @enFN: 
    <QUOTE::dis> Resource (in a narrow sense)
  @rdfs:subClassOf: DISCore|AnyResource
  @subsetOf: DISCore|AnyResource

ElementTypeBinding:
  @Name: ResourceTypeDef
  @ElementType: 
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type:
      rdfs:Class
    @@ISA:
      rdfs:Class

ElementTypeBinding:
  @Name: PropDef
  @ElementType:
    dis:ResourceDef
