    @@rdf:type:
      rdf:Property

ElementTypeBinding:
  @Name: SPropDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type: rdf|Property
    @@rdf:type: DISSource|Property
    @@rdf:type: DISSource|ResourceProperty

ElementTypeBinding:
  @Name: CPropDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type: DISCore|Property

ElementTypeBinding:
  @Name: dataType
  @ElementType:
    dis:dataType
  @ShadowContent:
    @@ContentType: DISCore|QName

ElementTypeBinding:
  @Name: multipleProperties
  @ElementType:
    dis:multipleProperties
  @ShadowContent:
    @@ContentType: DISCore|QName

## -- Any Modules

ResourceTypeDef:
  @QName: DISCore|AnyModule
  @enFN: Any Kind of Module
  @enDesc:
    A <Q::DISCore:AnyModule> is a module in some sense.  It might
    be a <Q::DISCore:Module> or might be a programming language module.
  @rdfs:subClassOf: DISCore|AnyResource
  @subsetOf: DISCore|AnyResource

CPropDef:
  @QName: DISCore|anyResource
  @enDesc:
    The subject resource has the object resource as a member.
  @multipleProperties: DISCore|UnorderedList
  @dataType: DISCore|TFQNames
  @rdfs:range: DISCore|AnyResource

SPropDef:
  @QName: DISCore|module
  @enDesc:
    The subject resource has the object module as a member.
  @multipleProperties: DISCore|UnorderedList
  @dataType: DISCore|ModuleTFQNames
  @rdfs:range: DISCore|Module
  @rdfs:subPropertyOf: DISCore|anyResource
  @subsetOf: DISCore|anyResource

SPropDef:
  @QName: DISCore|resource
  @enDesc:
    The subject resource has the object <QUOTE::dis> resource as a member.
  @multipleProperties: DISCore|UnorderedList
  @dataType: DISCore|TFQNames
  @rdfs:range: DISCore|Module
  @rdfs:subPropertyOf: DISCore|anyResource
  @subsetOf: DISCore|anyResource

## -- Module groups

ResourceTypeDef:
  @QName: 
    dis:ModuleGroup
  @Description:
    @@lang:en
    @@@: A set of modules
  @rdfs:subClassOf: DISCore|AnyModule
  @subsetOf: DISCore|AnyModule

ResourceDef:
  @QName:
    DIS:
  @rdf:type:
    dis:ModuleGroup
  @Description:
    @@lang:en
    @@@:
      The <QUOTE::dis> core library.
  @DISPerl:packageName: manakai

## -- Resource Names

CPropDef:
  @QName: DISCore|uri
  @enFN: URI
  @enDesc:
    A URI of the resource.  A <Q::DISCore:AnyResource> have
    zero or more URIs.

    Note that <Q::DISCore:For> have exact one URI by definition.
  @rdfs:domain: DISCore|AnyResource
  @multipleProperties: DISCore|UnorderedList

CPropDef:
  @QName: DISCore|nameURI
  @enFN: Name URI
  @enDesc:
    A name URI of the resource.  A <Q::DISCore:AnyResource>
    <kwd:MAY> have one or more name URIs, which might or might not be
    <Q::DISCore:uri>s of the resource.  Note that more than
    one <Q::DISCore:AnyResource>s might share the same
    <Q::DISCore:nameURI>.

    For <Q::DISCore:For> resources, the only name URI is the <Q::DISCore:uri>
    of the resource.
  @rdfs:domain: DISCore|AnyResource

CPropDef:
  @QName: DISCore|namespaceURI
  @enFN: Namespace URI
  @enDesc:
    A namespace URI of the resource.  Note that when a resource
    has a namespace URI, then the resource also has a <Q::DISCore:localName>
    associated to the namespace URI.
  @rdfs:domain: DISCore|AnyResource
  @rdfs:range: DISCore|Namespace

ResourceTypeDef:
  @QName: DISCore|Namespace
  @enFN: Namespace
  @rdfs:subClassOf: DISCore|Resource
  @subsetOf: DISCore|Resource

CPropDef:
  @QName: DISCore|localName
  @enFN: Local Name
  @enDesc:
    A local name of the resource.

    {NOTE:: More than one <Q::DISCore:AnyResource>s share the same
            local name in a namespace, as more than one
            <Q::DISCore:AnyResource>s share their name URI.
    }
  @rdfs:domain: DISCore|AnyResource

CPropDef:
  @QName: DISCore|AnyAppName
  @enFN: part of or entire name
  @enDesc:
    A name or a part of a name of the resource in some application.

CPropDef:
  @QName:
    dis:AppName
  @Description:
    @@lang:en
    @@@: An application (binding) dependent name of a resource.
  @dataType: String
  @multipleProperties: UnorderedList
  @rdfs:subPropertyOf: DISCore|AnyAppName
  @subsetOf: DISCore|AnyAppName

SPropDef:
  @QName:
    dis:FullName
  @Description:
    @@lang:en
    @@@:
      A human-readable name of a resource.
  @dataType: lang|disdocInline
  @multipleProperties: DISCore|OrderedList
  @rdfs:subClassOf: rdfs|label
  @subsetOf: rdfs|label

## -- Resource-to-resource relationships

CPropDef:
  @QName: derivedFrom
  @Description:
    @@lang:en
    @@@:
      A subject resource is in some way derived from an object resource. 
  @rdfs:domain:
    rdfs:Resource
  @rdfs:range:
    rdfs:Resource
  @dataType: DISCore|TFQNames
  @multipleProperties: OrderedList

SPropDef:
  @QName:
    dis:subsetOf
  @Description:
    @@lang:en
    @@@:
      A subject resource is a subset of an object resource. 
  @rdfs:domain:
    rdfs:Resource
  @rdfs:range:
    rdfs:Resource
  @subsetOf: derivedFrom
  @rdfs:subPropertyOf:derivedFrom
  @dataType: DISCore|TFQNames
  @multipleProperties: UnorderedList

CPropDef:
  @QName:
    dis:supersetOf
  @Description:
    @@lang:en
    @@@:
      The subject resource is a superset of the object resource. 
      \
      {NOTE:: This property is intended for internal use; it should 
              not be directly specified in the <Q::dis> files.
      \
      }
  @dataType: DISCore|TFQNames
  @multipleProperties: UnorderedList

SPropDef:
  @QName:
    dis:ISA
  @Description:
    @@lang:en
    @@@:
      Inheritance relationship of a resource.

    If a <Q::dis:ISA> element is specified as a child element
    of a <Q::dis:ForDef> element, then the <Q::dis:ISA> value
    defines a superresource of the <Q::DISCore:For> defined
    by the <Q::dis:ForDef> element.  The <Q::dis:ISA> value
    <kwd:MUST> be a <Q::DISCore:ForQName>.
  @rdf:type: DISCore|ElementType
  @subsetOf: derivedFrom
  @rdfs:subPropertyOf:derivedFrom
  @dataType: DISCore|TFQNames
  @multipleProperties: OrderedList

CPropDef:
  @QName:
    dis:AppISA
  @Description:
    @@lang:en
    @@@:
      Application dependent is-a relationship.

    (This property might be marked as obsolete in future revision.)
  @subsetOf: derivedFrom
  @rdfs:subPropertyOf:derivedFrom
  @multipleProperties: OrderedList
  @dataType: String

SPropDef:
  @QName:
    dis:Implement
  @Description:
    @@lang:en
    @@@:
      <QUOTE::Implementing> relationship of a resource.
  @subsetOf:derivedFrom
  @rdfs:subPropertyOf:derivedFrom
  @dataType: DISCore|TFQNames
  @multipleProperties: UnorderedList

SPropDef:
  @QName:
    ManakaiDOM:isRedefining
  @dataType: DISCore|Boolean
  @Description:
    @@lang:en
    @@@:
      Whether the subject resource (such as method or attribute) 
      redefines another resource defined in one of ancestor inheriting 
      class or not.  (Obsolete)
  @multipleProperties: Single

SPropDef:
  @QName:
    dis:IsAbstract
  @Description:
    @@lang:en
    @@@:
      Whether the resource is defined abstractly or concretly. 
      Abstractly defined resource must be concretly overloaded 
      by inheriting class.

    (Move to DISLang?)
  @dataType: dis|Boolean
  @multipleProperties: Single

SPropDef:
  @QName:
    dis:IsFinal
  @Description: 
    @@lang:en
    @@@:
      Whether a subject resource is overridable or not. 
      \
      - <CODE::-1> :: The subject resource have to be overrided.
      \
      - <CODE::0>  :: The subject resource is able to be overrided. 
      \
      - <CODE::1>  :: The subject resource is unable to be overrided.

    (Move to DISLang?)
  @dataType:
    DISCore:Tristate
  @multipleProperties: Single

## -- Datatypes

ResourceDef:
  @QName: AnyType
  @rdf:type:
    rdfs:Class
  @subsetOf:
    rdfs:Class
  @rdfs:subClassOf:
    rdfs:Class
  @Description:
    @@lang:en
    @@@:
      A data type, including programming language data types, 
      programming language classes, markup language data types, 
      string formats, and so on. 

ResourceDef:
  @QName: AbstractDataType
  @AliasFor: AnyType
  @enDesc: 
    The name of <QUOTE::abstract data type> was incorrect.

ResourceDef:
  @QName: LexicalType
  @rdf:type:
    rdfs:Class
  @subsetOf: AnyType
  @rdfs:subClassOf: AnyType
  @Description:
    @@lang:en
    @@@:
      Lexical data formats.

ResourceDef:
  @QName: StringDataType
  @AliasFor: LexicalType

SPropDef:
  @QName:
    dis:dataType
  @Description:
    @@lang:en
    @@@:
      Lexical data type of the property object.
  @rdfs:domain: DISSource|Property
  @rdfs:range: DISCore|LexicalType
  @dataType: DISCore|QName
  @multipleProperties: Single

PropDef:
  @QName:
    dis:ContentType
  @rdfs:range: LexicalType
  @rdf:type: DISCore|ElementType
  @rdf:type: DISSource|PropertyProperty
  @dataType: DISCore|QName
  @Description:
    @@lang:en
    @@@:
      The lexical data type of the content of e.g. a <QUOTE::dis> element.
  @multipleProperties: Single

ElementTypeBinding:
  @Name: enDesc
  @ElementType:
    dis:Description
  @ShadowContent:
    @@lang:en

ElementTypeBinding:
  @Name: enFN
  @ElementType:
    dis:FullName
  @ShadowContent:
    @@lang:en

## -- Lexical type instances

ElementTypeBinding:
  @Name: LexTypeDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type: DISCore|LexicalType

LexTypeDef:
  @QName: DISCore|AnyString
  @enFN: Context-dependent string
  @enDesc:
    A <Q::DISCore|AnyString> is a string, i.e. an ordered sequence
    of zero or more characters which might or might not depend
    on the context in which the string is evaluated.

LexTypeDef:
  @QName: DISCore|String
  @enFN: String
  @enDesc:
    A <Q::DISCore|String> is a string, i.e. an ordered sequence
    of zero or more characters which does not depend on
    any formal contextual information such as <QUOTE::dis>
    namespace bindings.

      {NOTE:: Even a <Q::DISCore|String> might depend on any
              context information that is not necessariliy 
              interpretable by machines; for example,
              a person who describes an object in <Q::DISCore|String>
              might assume that any other person who reads 
              the description knows what is the object described
              by it.
      }
  @rdfs:subClassOf: DISCore|AnyString
  @subsetOf: DISCore|AnyString

ResourceDef:
  @QName: dis|String
  @AliasFor: DISCore|String

LexTypeDef:
  @QName: DISCore|Text
  @enFN: Text
  @enDesc:
    A <Q::DISCore|Text> is a human readable plain text.
    <DFN::Human readable> means that the text is written
    in natural language or formal language that is intended
    for human consumption and it is expected that human users
    can be understood what is written in the text.

      {NOTE:: Even <QUPTE::plain> text sometimes contains
              a <QUOTE::preformatted> sequence of characters.
      }
  @rdfs:subClassOf: DISCore|String
  @subsetOf: DISCore|String

LexTypeDef:
  @QName: DISCore|Boolean
  @enFN: Boolean
  @enDesc:
    A <Q::DISCore|Boolean> is a Boolean truth value, either
    <CODE::1> or <CODE::0>.  This lexical type is semantically
    equals to <Q::DISLang|Boolean> and the values corresponding
    to <Q::DISLang|True> and <Q::DISLang|False>.
  @rdfs:subClassOf: DISCore|String
  @subsetOf: DISCore|String

ResourceDef:
  @QName: dis|Boolean
  @AliasFor: Boolean

LexTypeDef:
  @QName: DISCore|Tristate
  @enFN: Tristate
  @enDesc:
    A <Q::DISCore|Tristate> value ranges either <CODE::-1>, <QUOTE::0>,
    or <CODE::1>.  Although their semantics depends on the context 
    where they are used, it is expected that they represents 
    <Q::DISLang|False>, <Q::DISLang|Undefined>, and <Q::DISLang|True>
    repspecitvely.
  @rdfs:subClassOf: DISCore|String
  @subsetOf: DISCore|String

LexTypeDef:
  @QName: DISCore|Integer
  @enFN: Integer
  @enDesc:
    An <Q::DISCore|Integer> is a sequence of decimal digits,
    i.e. a sequence of characters <CHAR::DIGIT ZERO>, 
    <CHAR::DIGIT ONE>, ..., and <CHAR::DIGIT NINE>, 
    optionally preceded by a <CHAR::PLUS SIGN> or <CHAR::HYPHEN-MINUS>.
    They are interpreted as a decimal integer.
  @rdfs:subClassOf: DISCore|String
  @subsetOf: DISCore|String

LexTypeDef:
  @QName: dis|Date.RCS
  @FullName:
    @@lang:en
    @@@: RCS tag-style date and time specification
  @rdfs:subClassOf: DISCore|String
  @subsetOf: DISCore|String

LexTypeDef:
  @QName: DISCore|MailAddress
  @enFN: Internet mail address
  @rdfs:subClassOf: DISCore|String
  @subsetOf: DISCore|String

LexTypeDef:
  @QName: DISCore|DISString
  @enFN: 
    <QUOTE::dis>-context-dependent string
  @enDesc:
    {P:: A <Q::DISCore|DISString> is a <Q::DISCore|AnyString>
    that only depends on contextaul information provided
    in <QUOTE::dis> object model, i.e.

      - the set of namespace bindings (including prefixed namespaces
        and default namespace),

      - the base URI,

      - the default resource name URI, and
  
      - the default resource <QUOTE::for> URI.

    }
  @subsetOf: DISCore|AnyString
  @rdfs:subClassOf: DISCore|AnyString

LexTypeDef:
  @QName: DISCore|URI
  @enFN: URI
  @enDesc:
    A <Q::DISCore|URI> is a URI-like string that identifies any resource.

    A <Q::DISCore|URI> might be a relative reference; if so, 
    it might be resolved against the base URI of the context.

      {NOTE:: Typically <Q::DISCore|URI>s are not checked against
              syntactical or other constraints defined in URI
              standards.  In addition, relative references are
              not resolved.
      }
  @rdfs:subClassOf: DISCore|DISString
  @subsetOf: DISCore|DISString

LexTypeDef:
  @QName: DISCore|TFURI
  @enFN:
    <Q::DISCore|TFQNames>-expanded URI
  @enDesc:
    A <Q::DISCore|TFURI> is an absolute <Q::DISCore:URI> extracted
    from a <Q::DISCore|TFQNames>.

      {NOTE:: <Q::DISCore|TFURI>s and <Q::DISCore|TFQNames>s
              have the one-to-one relationship.
      }
  @rdfs:subClassOf: DISCore|URI
  @subsetOf: DISCore|URI

LexTypeDef:
  @QName: DISCore|TFPURI
  @enFN:
    <Q::DISCore|TFPQNames>-expanded URI
  @enDesc:
    A <Q::DISCore|TFPURI> is an absolute <Q::DISCore:URI> extracted
    from a <Q::DISCore|TFPQNames>.

      {NOTE:: <Q::DISCore|TFPURI>s and <Q::DISCore|TFPQNames>s
              have the one-to-one relationship.
      }
  @rdfs:subClassOf: DISCore|URI
  @subsetOf: DISCore|URI

LexTypeDef:
  @QName: DISCore|NSString
  @enFN: Namespace-binding-dependent string
  @enDesc:
    String, i.e. unconstrainted sequence of zero or more characters,
    which depends on namespace bindings.
  @rdfs:subClassOf: DISCore|DISString
  @subsetOf: DISCore|DISString

LexTypeDef:
  @QName: DISCore|NCName
  @FullName:
    @@lang:en
    @@@:
      <QUOTE::dis> NCName
  @rdfs:subClassOf: DISCore|AnyString
  @subsetOf: DISCore|AnyString

LexTypeDef:
  @QName: DISCore|NamespacePrefix
  @FullName:
    @@lang:en
    @@@:
      <QUOTE::dis> Namespace Prefix
  @rdfs:subClassOf: DISCore|NCName
  @rdfs:subClassOf: DISCore|NSString
  @subsetOf: DISCore|NCName
  @subsetOf: DISCore|NSString

LexTypeDef:
  @QName: LocalName
  @FullName:
    @@lang:en
    @@@:
      <QUOTE::dis> Local Name
  @rdfs:subClassOf: DISCore|NCName
  @rdfs:subClassOf: DISCore|String
  @subsetOf: DISCore|NCName
  @subsetOf: DISCore|String

LexTypeDef:
  @QName: DISCore|QName
  @FullName:
    @@lang:en
    @@@: 
      <QUOTE::dis> QName
  @Description:
    @@lang:en
    @@@:
      <QUOTE::dis> QName (qualified name).
      \
      <Q::dis:TypeQName> := [prefix ":"] local-name
      \
      prefix := *(<lt>any character> - ":")
      \
      local-name := *(<lt>any character> - ":")
  @rdfs:subClassOf: DISCore|NSString
  @subsetOf: DISCore|NSString

ResourceDef:
  @QName: dis|TypeQName
  @AliasFor: DISCore|QName

LexTypeDef:
  @QName: DISCore|ForQName
  @enFN: 
    <QUOTE::dis> QName Referencing a <Q::DISCore|For>
  @rdfs:subClassOf: DISCore|QName
  @subsetOf: DISCore|QName

LexTypeDef:
  @QName: DISCore|NCNameOrQName
  @enFN:
    <QUOTE::dis> NCName or <QUOTE::dis> QName with prefix
  @Description:
    @@lang:en
    @@@:
      A QName with or without <CHAR::COLON>.  If
      it does not contain a <CHAR::COLON>, it is namespace-unaware
      or in the null namespace.
  @rdfs:subClassOf: DISCore|QName
  @subsetOf: DISCore|QName

LexTypeDef:
  @QName: DISCore|TFQNames
  @enDesc:
    <QUOTE::dis> T & F QName pair
  @Description:
    @@lang:en
    @@@:
      A pair of resource QName and optional <QUOTE::For> QName.
  @rdfs:subClassOf: DISCore|DISString
  @subsetOf: DISCore|DISString

ResourceDef:
  @QName: dis|TFQNames
  @AliasFor: DISCore|TFQNames

LexTypeDef:
  @QName: DISCore|ModuleTFQNames
  @enFN:
    <QUOTE::dis> T & F QNames identifying a <Q::DISCore:Module>
  @rdfs:subClassOf: DISCore|TFQNames
  @subsetOf: DISCore|TFQNames

LexTypeDef:
  @QName: DISCore|TFPQNames
  @enFN:
    <QUOTE::dis> T & F & P QName triplet
  @Description:
    @@lang:en
    @@@:
      A triplet of resource QName and optional <QUOTE::For> QName
      and zero or more <QUOTE::For+> QNames.
  @rdfs:subClassOf: DISCore|DISString
  @subsetOf: DISCore|DISString

## -- Properties

ResourceTypeDef:
  @QName: DISCore|Property
  @enFN: Property
  @rdfs:subClassOf: rdf|Property
  @subsetOf: rdf|Property
  @ImplNote:
    @@lang:en
    @@@:
      Should a <Q::rdf|Property> be a <Q::DISCore|Property>?
      And vice versa?

PropDef:
  @QName: stopISARecursive
  @Description:
    @@lang:en
    @@@:
      Whether the <Q::dis:ISA>-recursive property search
      (i.e. <M::DIS:ManakaiDISPropertyAccessor.getPropertyResourceList>)
       should be stopped or not.
  @rdf:type: DISSource|PropertyProperty
  @dataType: DISCore|Boolean
  @multipleProperties: DISCore|Single

SPropDef:
  @QName: dis|multipleProperties
  @Description:
    @@lang:en
    @@@:
      This attribute defines that how the values should be interpreted
      if a property is described as multiple attributes in
      <QUOTE::dis> source tree.
  @rdfs:domain: rdf|Property
  @rdfs:range: rdfs|Resource
  @dataType: DISCore|QName
  @multipleProperties: Single

ResourceTypeDef:
  @QName: DISCore|PropertyValueOrder

ResourceDef:
  @QName: OrderedList
  @Description:
    @@lang:en
    @@@:
      If multiple values are specified to a property, then
      they makes up an ordered collection of values in document
      order.
  @rdf:type: DISCore|PropertyValueOrder

ResourceDef:
  @QName: UnorderedList
  @Description:
    @@lang:en
    @@@:
      If multiple valus are specified to a property, then
      they makes up an unordered collection of values.
  @rdf:type: DISCore|PropertyValueOrder

ResourceDef:
  @QName: Single
  @Description:
    @@lang:en
    @@@:
      Only a value is allowed for the property; if there
      are more than one attributes in the source tree, it is an error.
  @rdf:type: DISCore|PropertyValueOrder

