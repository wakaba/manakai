ElementTypeBinding:
  @Name: DataTypeDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type:
      DISCore:AbstractDataType
    @@AliasFor:
      @@@@:
        ::ManakaiDOM:all
      @@@For:
        !=ManakaiDOM:all

ElementTypeBinding:
  @Name: PropDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type:
      rdf:Property
    @@AliasFor:
      @@@@:
        ::ManakaiDOM:all
      @@@For:
        !=ManakaiDOM:all

ElementTypeBinding:
  @Name: FormatDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type:
      DISCore:StringDataType
    @@AliasFor:
      @@@@:
        ::ManakaiDOM:all
      @@@For:
        !=ManakaiDOM:all
    @@rdfs:subClassOf:
      DISCore:String

## -- The lang:dis tree language vocabulary

DataTypeDef:
  @QName:
    lang:dis
  @FullName:
    @@lang:en
    @@@:
      The <QUOTE:dis> format

ResourceDef:
  @Name: TreeElementType
  @rdf:type:
    rdfs:Class
  @rdfs:subClassOf:
    dis:ElementType
  @Description:
    @@lang:en
    @@@:
      A <Q::lang:dis> tree vocabulary element type.
  @AliasFor:
    @@@:
      ::ManakaiDOM:all
    @@For:
      !=ManakaiDOM:all

ElementTypeBinding:
  @Name: TreeElementType
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type: TreeElementType
    @@AliasFor:
      @@@@:
        ::ManakaiDOM:all
      @@@For:
        !=ManakaiDOM:all

TreeElementType:
  @QName: nop
  @FullName:
    @@lang:en
    @@@:
      No operation

TreeElementType:
  @Name: constValue
  @Description:
    @@lang:en
    @@@:
      Returns a constant value.

TreeElementType:
  @Name: dataTypeAliasFor
  @rdf:type:
    rdf:Property
  @Description:
    @@lang:en
    @@@:
      Declares the subject resource is an alias for the object resource.
  @rdfs:range:
    DISCore:AbstractDataType
  @rdfs:domain:
    DISCore:AbstractDataType
  @dataType: 
    dis:TFQNames

TreeElementType:
  @Name: value
  @Description:
    @@lang:en
    @@@: Returns a value.

TreeElementType:
  @QName: 
    DISPerl:selectByProp
  @Description:
    @@lang:en
    @@@:
      Static (convertion time) selection, from code fragments, 
      by the value of a (static) property.

TreeElementType:
  @QName: 
    DISPerl:propName
  @Description:
    @@lang:en
    @@@:
      Property name.
  @dataType:
    dis:TypeQName
  @rdfs:domain: 
    DISPerl:selectByProp
  @rdf:type:
    rdf:Property

TreeElementType:
  @QName: 
    DISPerl:case
  @Description:
    @@lang:en
    @@@:
      A <QUOTE::case>. 
  @rdfs:domain:
    DISPerl:selectByProp

TreeElementType:
  @QName:
    DISPerl:else
  @Description:
    @@lang:en
    @@@:
      An <QUOTE::otherwise> case.
  @rdfs:domain:
    DISPerl:selectByProp

