Module:
  @QName: 
    DIS:DISRDF
  @FullName:
    @@lang:en
    @@@: 
      <QUOTE::dis> RDF and OWL Module
  @Namespace:
    http://suika.fam.cx/~wakaba/archive/2004/dis/RDF#

  @Description:
    @@lang:en
    @@@:
      This module defines a number of vocabulary from RDF and OWL which 
      is considered useful in the <QUOTE::dis> contexts. 
      \
      {NOTE:: For the formal definition of properties and classes 
              defined in this module, see RDF and OWL specifications 
              published by the W3C. 
      \
      }
  
  @DISCore:author: DISCore|Wakaba
  @License:
     license:Perl+MPL
  @Date:
    $Date: 2006/12/31 09:12:03 $

  @Require:
    @@Module:
      @@@QName:
        DIS:DISCore
    @@Module:
      @@@QName:
        DIS:XML
    @@Module:
      @@@QName: DIS|DISSource

Namespace:
  @DIS:
    http://suika.fam.cx/~wakaba/archive/2004/dis/
  @dis:
    http://suika.fam.cx/~wakaba/archive/2004/8/18/lang#dis--
  @DISCore:
    http://suika.fam.cx/~wakaba/archive/2004/dis/Core#
  @foaf:
    http://xmlns.com/foaf/0.1/
  @infoset:
    http://www.w3.org/2001/04/infoset#
  @lang:
    http://suika.fam.cx/~wakaba/archive/2004/8/18/lang#
  @license:
    http://suika.fam.cx/~wakaba/archive/2004/8/18/license#
  @ManakaiDOM:
    http://suika.fam.cx/~wakaba/archive/2004/8/18/manakai-dom#
  @owl:
    http://www.w3.org/2002/07/owl#
  @rdf:
    http://www.w3.org/1999/02/22-rdf-syntax-ns#
  @rdfs:
    http://www.w3.org/2000/01/rdf-schema#

ResourceDef:
  @QName:
    rdfs:Resource
  @DISCore:resourceType: rdfs|Class
  @rdf:type:
    rdfs:Class
  @Description:
    @@lang:en
    @@@: An RDF resource.
  @rdfs:subClassOf: DISCore|AnyResource
  @subsetOf: DISCore|AnyResource

ResourceDef:
  @QName:
    owl:Thing
  @DISCore:resourceType: rdfs|Class
  @rdf:type:
    owl:Class
  @Description:
    @@lang:en
    @@@:
      All individuals.  Every OWL class is a subclass of <Q::owl:Thing>. 

ResourceDef:
  @QName:
    owl:Nothing
  @DISCore:resourceType: rdfs|Class
  @rdf:type:
    owl:Class
  @Description:
    @@lang;en
    @@@:
      The empty set.  Note that <Q::owl:Nothing> is a subclass of every 
      class. 

ResourceDef:
  @QName:
    rdfs:Class
  @DISCore:resourceType: rdfs|Class
  @rdf:type:
    rdfs:Class
  @subsetOf:
    rdfs:Resource
  @rdfs:subClassOf:
    rdfs:Resource
  @Description:
    @@lang:en
    @@@: A RDF class.
  @subsetOf: DISCore|ResourceType
  @rdfs:subClassOf: DISCore|ResourceType
  @ImplNote:
    @@lang:en
    @@@:
      Future version should remove this <Q::subsetOf> relationship.

ResourceDef:
  @QName:
    owl:Class
  @DISCore:resourceType: owl|Class
  @rdf:type:
    owl:Class
  @subsetOf:
    rdfs:Class
  @rdfs:subClassOf:
    rdfs:Class
  @Description:
    @@lang:en
    @@@:
      OWL classes.

ResourceDef:
  @QName:
    owl:DeprecatedClass
  @DISCore:resourceType: rdfs|Class
  @rdf:type:
    rdfs:Class
  @subsetOf:
    rdfs:Class
  @rdfs:subClassOf:
    rdfs:Class
  @Description:
    @@lang:en
    @@@:
      A deprecated class. 

ResourceDef:
  @QName:
    rdfs:Literal
  @DISCore:resourceType: rdfs|Class
  @rdf:type:
    rdfs:Class
  @rdfs:subClassOf:
    rdfs:Class
  @subsetOf:
    rdfs:Class
  @rdfs:subClassOf:
    infoset:Literal
  @subsetOf:
    infoset:Literal

ResourceDef:
  @QName:
    rdfs:Datatype
  @DISCore:resourceType: rdfs|Class
  @rdf:type:
    rdfs:Class
  @rdfs:subClassOf:
    rdfs:Class
  @subsetOf: rdfs|Class

ResourceDef:
  @QName:
    rdf:Property
  @Description:
    @@lang:en
    @@@: An RDF property.
  @DISCore:resourceType: rdfs|Class
  @rdf:type:
    rdfs:Class
  @subsetOf:
    rdfs:Class
  @rdfs:subClassOf:
    rdfs:Class

ElementTypeBinding:
  @Name: PropDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@DISCore:resourceType: rdf|Property
    @@rdf:type:
      rdf:Property

ElementTypeBinding:
  @Name: SPropDef
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type: rdf|Property
    @@DISCore:resourceType: rdf|Property
    @@DISCore:resourceType: DISSource|Property
    @@DISCore:resourceType: DISSource|ResourceProperty

ResourceDef:
  @QName:
    owl:DeprecatedProperty
  @DISCore:resourceType: rdfs|Class
  @rdf:type:
    rdfs:Class
  @rdfs:subClassOf:
    rdf:Property
  @subsetOf:
    rdf:Property
  @Description:
    @@lang:en
    @@@:
      Deperecated properties. 

SPropDef:
  @QName:
    rdf:type
  @rdfs:domain:
    rdfs:Resource
  @rdfs:range:
    rdfs:Class
  @subsetOf: DISCore|resourceType
  @dataType: DISCore|QName
  @multipleProperties: DISCore|UnorderedList

SPropDef:
  @QName:
    rdfs:subClassOf
  @Description:
    @@lang:en
    @@@:
      The subject is a subclass of a class.
  @rdfs:domain:
    rdfs:Class
  @rdfs:range:
    rdfs:Class
  @subsetOf:
    dis:subsetOf
  @dataType: DISCore|QName
  @multipleProperties: DISCore|UnorderedList

SPropDef:
  @QName:
    rdfs:subPropertyOf
  @Description:
    @@lang:en
    @@@:
      The subject is a subproperty of a property.
  @rdfs:domain:
    rdf:Property
  @rdfs:range:
    rdf:Property
  @subsetOf:
    dis:subsetOf
  @dataType: DISCore|QName
  @multipleProperties: DISCore|UnorderedList

SPropDef:
  @QName:
    owl:sameAs
  @Description:
    @@lang:en
    @@@:
      The subject is same as the object.
  @dataType: DISCore|QName
  @multipleProperties: DISCore|UnorderedList

SPropDef:
  @QName:
    rdfs:domain
  @rdfs:domain:
    rdf:Property
  @rdfs:range:
    rdfs:Class
  @dataType: DISCore|QName
  @multipleProperties: DISCore|UnorderedList

SPropDef:
  @QName:
    rdfs:range
  @rdfs:domain:
    rdf:Property
  @rdfs:range:
    rdfs:Class
  @dataType: DISCore|QName
  @multipleProperties: DISCore|UnorderedList

PropDef:
  @QName:
    rdfs:label
  @Description:
    @@lang:en
    @@@: A human-readable name of the subject resource.
  @rdfs:domain:
    rdfs:Resource
  @rdfs:range:
    rdfs:Literal
  @dataType: DISCore|String
  @multipleProperties: DISCore|OrderedList

PropDef:
  @QName:
    rdfs:comment
  @Description:
    @@lang:en
    @@@: A description of the subject resource.
  @rdfs:domain:
    rdfs:Resource
  @rdfs:range:
    rdfs:Literal
  @dataType: DISCore|String
  @multipleProperties: DISCore|OrderedList

ResourceDef:
  @QName:
    rdf:Statement
  @Description:
    @@lang:en
    @@@:
      RDF statements.
  @DISCore:resourceType: rdfs|Class
  @rdf:type:
    rdfs:Class
  @rdfs:subClassOf:
    rdfs:subClassOf
  @subsetOf:
    rdfs:subClassOf

PropDef:
  @QName:
    rdfs:subject
  @Description:
    @@lang:en
    @@@:
      The subject of an RDF statement. 
  @rdfs:domain:
    rdf:Statement
  @rdfs:range:
    rdfs:Resource
  @dataType: DISCore|QName
  @multipleProperties: DISCore|Single

PropDef:
  @QName:
    rdfs:predicate
  @Description:
    @@lang:en
    @@@:
      The predicate of an RDF statement. 
  @rdfs:domain:
    rdf:Statement
  @rdfs:range:
    rdfs:Resource
  @dataType: DISCore|QName
  @multipleProperties: DISCore|Single

PropDef:
  @QName:
    rdfs:object
  @Description:
    @@lang:en
    @@@:
      The object of an RDF statement. 
  @rdfs:domain:
    rdf:Statement
  @rdfs:range:
    rdfs:Resource
  @dataType: DISCore|QName
  @multipleProperties: DISCore|Single

PropDef:
  @QName:
    rdfs:seeAlso
  @Description:
    @@lang:en
    @@@:
      An additional information on the subject. 
  @rdfs:domain:
    rdfs:Resource
  @rdfs:range:
    rdfs:Resource
  @dataType: DISCore|QName
  @multipleProperties: DISCore|OrderedList

PropDef:
  @QName:
    rdfs:isDefinedBy
  @Description:
    @@lang:en
    @@@:
      An RDF vocabulary in which the subject is described. 
  @rdfs:domain:
    rdfs:Resource
  @rdfs:range:
    rdfs:Resource
  @dataType: DISCore|QName
  @multipleProperties: DISCore|OrderedList

PropDef:
  @QName:
    rdf:value
  @Description:
    @@lang:en
    @@@:
      The primary value of a structured resource. 
  @rdfs:domain:
    rdfs:Resource
  @rdfs:range:
    rdfs:Resource
  @dataType: DISCore|String
  @multipleProperties: DISCore|OrderedList

ElementTypeBinding:
  @Name: PropType
  @ElementType:
    dis:ResourceDef
  @ShadowContent:
    @@rdf:type:
      rdfs:Class
    @@DISCore:resourceType: rdfs|Class
    @@rdfs:subClassOf:
      rdf:Property
    @@subsetOf:
      rdf:Property

PropType:
  @QName:
    owl:FunctionalProperty
  @Description:
    @@lang:en
    @@@:
      Functional (<VAR::n> to one) properties. 

PropType:
  @QName:
    owl:InverseFunctionalProperty
  @Description:
    @@lang:en
    @@@:
      Inverse-functional properties. 

PropType:
  @QName:
    owl:SymmetricProperty
  @Description:
    @@lang:en
    @@@:
      Symmetric properties.  A property <VAR::P> is symmetric iff 
      <MATH::<VAR::x> <VAR::P> <VAR::y> =<gt> <VAR::y> <VAR::P> <VAR::x>>. 

PropType:
  @QName:
    owl:TransitiveProperty
  @Description:
    @@lang:en
    @@@:
      Transitive properties.  A property <VAR::P> is transitive iff 
      <MATH::<VAR::x> <VAR::P> <VAR::y> and <VAR::y> <VAR::P> <VAR::z> 
             =<gt> <VAR::x> <VAR::P> <VAR::z>>. 

## -- Containers

ResourceDef:
  @QName:
    rdf:Bag
  @rdf:type:
    rdfs:Class
  @DISCore:resourceType: rdfs|Class

ResourceDef:
  @QName:
    rdf:Seq
  @rdf:type:
    rdfs:Class
  @DISCore:resourceType: rdfs|Class

## -- From FOAF

ResourceDef:
  @QName: foaf|Agent
  @enFN: Agent
  @rdfs:subClassOf: rdfs|Class
  @subsetOf: rdfs|Class
  @rdf:type: rdfs|Class
  @DISCore:resourceType: rdfs|Class

ResourceDef:
  @QName: foaf|Person
  @enFN: Person
  @rdfs:subClassOf: foaf|Agent
  @subsetOf: foaf|Agent
  @rdf:type: rdfs|Class
  @DISCore:resourceType: rdfs|Class

ResourceDef:
  @QName: foaf|Group
  @enFN: Group
  @rdfs:subClassOf: foaf|Agent
  @subsetOf: foaf|Agent
  @rdf:type: rdfs|Class
  @DISCore:resourceType: rdfs|Class

SPropDef:
  @QName: foaf|name
  @enFN: Name
  @rdfs:subPropertyOf: rdf|label
  @subsetOf: rdf|label
  @multipleProperties: DISCore|UnorderedList
  @dataType: lang|disdocInline

SPropDef:
  @QName: foaf|mbox
  @enFN: Mailbox
  @multipleProperties: DISCore|UnorderedList
  @dataType: DISCore|MailAddress

ElementTypeBinding:
  @Name: enFN
  @ElementType:
    dis:FullName
  @ShadowContent:
    @@lang:en
